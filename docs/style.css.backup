}

.social-link-btn:hover {
  transform: translateY(-4px) translateX(-2px) !important;
  box-shadow: 2px 5px 0 0 black !important;
}

.social-link-btn:active {
  transform: translateY(2px) translateX(1px) !important;
  box-shadow: 0 0 0 0 black !important;
}

/* Tablet */
@media (max-width: 1024px) {
  .social-grid {
    grid-template-columns: repeat(2, 1fr);
  }
}

/* Mobile */
@media (max-width: 640px) {
  .social-section {
    padding: 60px 0;
  }
  
  .social-grid {
    grid-template-columns: repeat(2, 1fr);
    gap: 12px;
  }
  
  .social-link-btn {
    padding: 14px 32px;
    font-size: 1rem;
  }
}

/* ===== CUSTOMER REVIEWS SECTION ===== */

.customer-reviews-section {
  padding: 80px 0;
  background: var(--milk-white);
}

.reviews-section-header {
  text-align: center;
  margin-bottom: 56px;
}

.reviews-section-header h2 {
  font-size: clamp(1.9rem, 3.8vw, 2.4rem);
  color: var(--espresso);
  margin-bottom: 12px;
}

.reviews-section-sub {
  max-width: 640px;
  margin: 0 auto;
  color: var(--text-muted);
  font-size: 1.1rem;
}

/* Reviews Summary */
.reviews-summary {
  display: flex;
  gap: 60px;
  max-width: 900px;
  margin: 0 auto 60px;
  padding: 40px;
  background: var(--white);
  border-radius: 20px;
  box-shadow: 0 4px 20px rgba(15,23,42,0.08);
}

.reviews-summary-left {
  flex-shrink: 0;
}

.average-rating {
  display: flex;
  align-items: center;
  gap: 20px;
}

.rating-number {
  font-size: 4rem;
  font-weight: 700;
  color: var(--espresso);
  line-height: 1;
}

.rating-details {
  display: flex;
  flex-direction: column;
  gap: 8px;
}

.rating-stars-large {
  color: #fbbf24;
  font-size: 1.5rem;
  letter-spacing: 2px;
}

.rating-text {
  margin: 0;
  font-size: 0.95rem;
  color: var(--text-muted);
}

.reviews-summary-right {
  flex: 1;
  display: flex;
  flex-direction: column;
  gap: 16px;
}

.rating-breakdown {
  display: flex;
  flex-direction: column;
  gap: 12px;
}

.rating-bar-row {
  display: flex;
  align-items: center;
  gap: 12px;
}

.star-label {
  font-size: 0.9rem;
  font-weight: 600;
  color: var(--espresso);
  min-width: 32px;
}

.rating-bar-container {
  flex: 1;
  height: 8px;
  background: #e5e7eb;
  border-radius: 4px;
  overflow: hidden;
}

.rating-bar-fill {
  height: 100%;
  background: linear-gradient(90deg, #fbbf24, #f59e0b);
  border-radius: 4px;
  transition: width 0.3s ease;
}

.rating-count {
  font-size: 0.85rem;
  color: var(--text-muted);
  min-width: 50px;
  text-align: right;
}

.recommendation-text {
  margin: 8px 0 0;
  padding-top: 16px;
  border-top: 1px solid rgba(62,39,35,0.1);
  font-size: 0.95rem;
  font-weight: 600;
  color: var(--espresso);
}

.reviews-carousel-wrapper {
  overflow-x: auto;
  overflow-y: hidden;
  -webkit-overflow-scrolling: touch;
  scrollbar-width: thin;
  scrollbar-color: rgba(62,39,35,0.2) transparent;
  padding: 0 0 20px 0;
  scroll-behavior: smooth;
}

@media (max-width: 640px) {
  .reviews-carousel-wrapper {
    scroll-snap-type: x mandatory;
    scrollbar-width: none;
  }
  
  .reviews-carousel-wrapper::-webkit-scrollbar {
    display: none;
  }
}

.reviews-carousel-wrapper::-webkit-scrollbar {
  height: 8px;
}

.reviews-carousel-wrapper::-webkit-scrollbar-track {
  background: transparent;
}

.reviews-carousel-wrapper::-webkit-scrollbar-thumb {
  background: rgba(62,39,35,0.2);
  border-radius: 4px;
}

.reviews-carousel-wrapper::-webkit-scrollbar-thumb:hover {
  background: rgba(62,39,35,0.3);
}

.reviews-carousel {
  display: flex;
  gap: 24px;
  padding: 0 max(20px, calc((100vw - 1200px) / 2));
}

.customer-review-card {
  background: var(--white);
  padding: 32px;
  border-radius: 16px;
  box-shadow: 0 4px 16px rgba(15,23,42,0.06);
  transition: transform 0.3s ease, box-shadow 0.3s ease;

}

/* Mobile Responsive */
@media (max-width: 768px) {
    .nutrition-grid {
        grid-template-columns: 1fr;
        gap: 24px;
    }
    
    .nutrition-card {
        padding: 24px;
    }
    
    .regulatory-info {
        padding: 24px;
    }
    

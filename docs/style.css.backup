  height: 48px;
  border-radius: 50%;
  background: radial-gradient(circle at 30% 20%, #ffe7c9, #f4a65a);
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: 700;
  font-size: 0.9rem;

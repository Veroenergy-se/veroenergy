/* Polaroid styles moved to inline in index.html */

/* ===== Our Story: polaroid + text layout ===== */
.vero-polaroid-section {
  position: relative;
  z-index: 10;
  max-width: 1100px;
  margin: 0 auto;
  padding: 4rem 2rem;
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 4rem;
  color: #2b1b23;
}

@media (max-width: 900px) {
  .vero-polaroid-section {
    flex-direction: column;
    align-items: center;
    text-align: center;
    gap: 2.5rem;
  }
}

.vero-polaroid-stack {
  position: relative;
  width: 450px;
  height: 550px;
  flex: 0 0 auto;
}

.vero-polaroid {
  position: relative;
  width: 100%;
  height: 100%;
  background: #f8f3e6;
  border: 3px solid #111;
  padding: 25px 25px 70px 25px;
  box-sizing: border-box;
  z-index: 3;
  transform: translate(12px, 12px) rotate(2deg);
}

.vero-polaroid::before,
.vero-polaroid::after {
  content: "";
  position: absolute;
  inset: 0;
  background: #f8f3e6;
  border: 3px solid #111;
  box-sizing: border-box;
  z-index: -1;
}

.vero-polaroid::before {
  transform: translate(-12px, -12px) rotate(-6deg);
}

.vero-polaroid::after {
  transform: translate(12px, 12px) rotate(6deg);
}

.vero-polaroid-inner {
  width: 100%;
  height: 100%;
  background: #f8f3e6;
  padding: 0;
  box-sizing: border-box;
  position: relative;
}

.vero-polaroid-photo {
  width: 100%;
  height: 100%;
  background: transparent;
  border: 2px solid #111;
  box-sizing: border-box;
}

.vero-story-frame {
  position: relative;
  flex: 1;
  max-width: 1000px;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  background: #f8f3e6;
  border: 3px solid #111;
  padding: 4rem 5rem 5rem;
}

.vero-story-frame::after {
  content: "";
  position: absolute;
  top: 10px;
  left: 10px;
  right: -10px;
  bottom: -10px;
  background: #111;
  z-index: -1;
}

.vero-story-frame h2,
.vero-story-frame p {
  position: relative;
  z-index: 2;
}

.vero-story-frame h2 {
  display: block;
  background: transparent;
  border: none;
  padding: 0;
  margin: 0 0 2rem 0;
  font-size: 3.5rem;
  font-weight: 700;
  color: #000 !important;
  font-family: 'Caveat', cursive !important;
}

.vero-story-frame p {
  background: transparent;
  border: none;
  padding: 0;
  margin: 0;
  font-size: 1.1rem;
  line-height: 1.8;
  color: #000 !important;
  font-weight: 400;
  font-family: 'Merriweather', serif !important;
}

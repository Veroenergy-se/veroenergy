}
@media (max-width:900px){ .sp-band{ grid-template-columns: repeat(2,1fr); } }
@media (max-width:520px){ .sp-band{ grid-template-columns:1fr; } }

.sp-stat{ text-align:center; padding:8px 6px; }
.sp-num{
  font-weight:900; letter-spacing:.02em;
  background:linear-gradient(90deg,#8b5cf6,#7c3aed);
  -webkit-background-clip:text; background-clip:text; color:transparent;
  font-size: clamp(1.4rem, 4vw, 2rem);
}
.sp-label{ color:#94a3b8; text-transform:uppercase; letter-spacing:.08em; font-size:.8rem; margin-top:2px; }

/* ===== NUTRITION (nt-)  detailed, interactive, on-brand ===== */
:root{
  --nt-purple:#8b5cf6;
  --nt-purple-deep:#6d28d9;
  --nt-gold:#f59e0b;
  --nt-gold-2:#fbbf24;
  --nt-ink:#0f172a;
  --nt-muted:#64748b;
  --nt-bg:#fbfbfd;
  --nt-card:#ffffff;
  --nt-border:rgba(2,6,23,.08);
  --nt-shadow-sm:0 10px 24px rgba(2,6,23,.06);
  --nt-shadow-md:0 14px 32px rgba(2,6,23,.10);
  --nt-r:18px;
}

.nt-section{
  padding:96px 0 110px;
  background:
    radial-gradient(1200px 420px at 12% -8%, rgba(139,92,246,.10), transparent 60%),
    radial-gradient(1000px 380px at 88% -10%, rgba(245,158,11,.08), transparent 60%),
    var(--nt-bg);
  color:var(--nt-ink);
}

/* header */
.nt-head{ text-align:center; max-width:820px; margin:0 auto 26px; }
.nt-badge{
  display:inline-block; padding:6px 12px; border-radius:999px; font-weight:800; font-size:.86rem;
  background:rgba(124,58,237,.12); color:var(--nt-purple-deep); border:1px solid rgba(124,58,237,.28);
}
.nt-title{ font-size:clamp(1.9rem,4vw,2.5rem); margin:10px 0 6px; }
.nt-sub{ color:var(--nt-muted); }

/* macros */
.nt-macros{
  display:grid; grid-template-columns:repeat(4, minmax(200px,1fr));
  gap:16px; margin-top:18px;
}
@media (max-width:980px){ .nt-macros{ grid-template-columns:repeat(2,1fr); } }
@media (max-width:520px){ .nt-macros{ grid-template-columns:1fr; } }

.nt-macro{
  background:var(--nt-card); border:1px solid var(--nt-border); border-radius:var(--nt-r); padding:16px;
  box-shadow:var(--nt-shadow-sm); text-align:center; transition:transform .2s ease, box-shadow .2s ease;
}
.nt-macro:hover{ transform:translateY(-4px); box-shadow:var(--nt-shadow-md); }
.nt-circle{
  width:86px; height:86px; border-radius:999px; display:grid; place-items:center; margin:0 auto 8px;
  color:#fff; font-weight:900; letter-spacing:.02em; font-size:1.2rem;
}
.nt-protein{ background:linear-gradient(135deg,var(--nt-purple),#6366f1); }
.nt-fiber{ background:linear-gradient(135deg,#22c55e,#10b981); }
.nt-sugar{ background:linear-gradient(135deg,#ef4444,#f97316); }
.nt-vegan{ background:linear-gradient(135deg,var(--nt-gold),var(--nt-gold-2)); color:#231f15; }
.nt-m-label{ font-weight:800; margin-bottom:2px; }
.nt-m-copy{ color:#475569; margin:0; }

/* benefit cards */
.nt-grid{
  margin-top:22px; display:grid; grid-template-columns:repeat(4, minmax(230px,1fr)); gap:18px;
}
@media (max-width:1100px){ .nt-grid{ grid-template-columns:repeat(2, minmax(230px,1fr)); } }
@media (max-width:640px){ .nt-grid{ grid-template-columns:1fr; } }

.nt-card{
  background:var(--nt-card); border:1px solid var(--nt-border); border-radius:var(--nt-r); padding:18px;
  box-shadow:var(--nt-shadow-sm); transition:transform .2s ease, box-shadow .2s ease, border-color .2s ease;
  display:flex; flex-direction:column; gap:12px;
}
.nt-card:hover{ transform:translateY(-4px); box-shadow:var(--nt-shadow-md); }
.nt-eyebrow{
  display:inline-block; padding:4px 10px; border-radius:999px; font-size:.75rem; font-weight:800; letter-spacing:.04em; color:#fff;
  background:linear-gradient(135deg,var(--nt-purple),#6366f1); margin-bottom:8px;
}
.nt-card h3{ margin:4px 0 0; }
.nt-card-summary{ color:#1f2937; font-weight:600; margin:0 0 4px; }
.nt-card .nt-more-btn{ align-self:flex-start; }

.nt-more-btn{
  display:inline-flex;
  align-items:center;
  gap:6px;
  padding:8px 14px;
  border-radius:999px;
  border:1px solid rgba(124,58,237,.28);
  background:rgba(124,58,237,.08);
  color:var(--nt-purple-deep);
  font-weight:600;
  font-size:.85rem;
  cursor:pointer;
  transition:background .2s ease, color .2s ease, box-shadow .2s ease, transform .2s ease, border-color .2s ease;
}
.nt-more-btn::after{
  content:'';
  font-weight:700;
  transition:transform .2s ease;
}
.nt-more-btn:hover,
.nt-more-btn:focus{
  background:linear-gradient(135deg,var(--nt-purple),#6366f1);
  border-color:transparent;
  color:#fff;
  box-shadow:var(--nt-shadow-sm);
  transform:translateY(-1px);
}
.nt-more-btn:hover::after,
.nt-more-btn:focus::after{ transform:translateX(2px); }
.nt-more-btn:focus-visible{
  outline:2px solid rgba(124,58,237,.5);
  outline-offset:2px;
}

/* ingredient highlights */
.nt-highlights{
  margin-top:22px; display:grid; grid-template-columns:repeat(3,1fr); gap:16px;
}
@media (max-width:900px){ .nt-highlights{ grid-template-columns:1fr; } }

.nt-hi-card{
  background:var(--nt-card); border:1px solid var(--nt-border); border-radius:var(--nt-r); padding:18px;
  box-shadow:var(--nt-shadow-sm);
  display:flex; flex-direction:column; gap:10px;
}
.nt-hi-card h4{ margin:0; }
.nt-hi-summary{ color:var(--nt-muted); margin:0; font-weight:500; }
.nt-hi-card .nt-more-btn{ align-self:flex-start; }

/* nutrition facts preview */
.nt-facts-wrap{
  display:grid; grid-template-columns: 360px 1fr; gap:18px; align-items:start; margin-top:24px;
}
@media (max-width:900px){ .nt-facts-wrap{ grid-template-columns:1fr; } }

.nt-facts{
  background:#fff; border:1px solid var(--nt-border); border-radius:12px; box-shadow:var(--nt-shadow-sm);
  font-family: ui-sans-serif, system-ui, -apple-system, Segoe UI, Roboto, "Helvetica Neue", Arial;
}
.nt-facts header{
  font-weight:900; letter-spacing:.02em; padding:10px 12px; border-bottom:1px solid var(--nt-border);
}
.nt-row, .nt-subrow{
  display:flex; justify-content:space-between; align-items:center; padding:8px 12px; color:#0f172a;
}
.nt-subrow{ padding-left:24px; color:#334155; }
.nt-facts hr{ border:none; border-top:1px solid var(--nt-border); margin:0; }
.nt-facts footer{ color:#94a3b8; font-size:.86rem; padding:10px 12px; border-top:1px solid var(--nt-border); }

.nt-facts-note{
  background:linear-gradient(180deg,#0f172a,#111827); color:#e5e7eb; border-radius:14px; padding:16px;
  box-shadow:0 18px 42px rgba(0,0,0,.25);
  border:1px solid rgba(255,255,255,.06);
}
.nt-facts-note h4{
  margin:0 0 8px; background:linear-gradient(90deg,var(--nt-gold),var(--nt-gold-2));
  -webkit-background-clip:text; background-clip:text; color:transparent;
}

/* FAQ */
.nt-faq{ margin-top:22px; display:grid; gap:10px; max-width:900px; margin-inline:auto; }
.nt-faq details{
  background:#fff; border:1px solid var(--nt-border); border-radius:12px; padding:12px 14px; color:#334155;
}
.nt-faq summary{ cursor:pointer; font-weight:700; }
.nt-faq details[open]{ box-shadow:var(--nt-shadow-sm); }

/* ===== NUTRITION: Quick Compare ===== */
.nt-compare{
  margin-top: 24px;
  background: #fff;
  border: 1px solid var(--nt-border);
  border-radius: 18px;
  box-shadow: var(--nt-shadow-sm);
  padding: 18px;
}

.nt-compare-title{
  margin: 0 0 12px;
  text-align: center;
  font-size: clamp(1.2rem, 2.5vw, 1.4rem);
  background: linear-gradient(90deg, var(--nt-gold), var(--nt-gold-2));
  -webkit-background-clip: text; background-clip: text; color: transparent;
  font-weight: 900;
  letter-spacing: .02em;
}

.nt-compare-grid{
  display: grid;
  grid-template-columns: repeat(3, minmax(220px,1fr));
  gap: 14px;
}
@media (max-width: 900px){ .nt-compare-grid{ grid-template-columns: 1fr; } }

.nt-compare-card{
  background: #fff;
  border: 1px solid var(--nt-border);
  border-radius: 14px;
  padding: 14px;
  box-shadow: 0 10px 22px rgba(2,6,23,.05);
  transition: transform .2s ease, box-shadow .2s ease, border-color .2s ease;
}
.nt-compare-card:hover{
  transform: translateY(-3px);
  box-shadow: 0 14px 30px rgba(2,6,23,.08);
}

.nt-compare-card.good{
  border-color: rgba(245,158,11,.35);
  box-shadow: 0 14px 28px rgba(245,158,11,.12);
}

.nt-co-eyebrow{
  display: inline-block;
  padding: 4px 10px;
  border-radius: 999px;
  font-size: .78rem;
  font-weight: 800;
  letter-spacing: .04em;
  color: #fff;
  background: linear-gradient(135deg, var(--nt-purple), #6366f1);
  margin-bottom: 8px;
}
.nt-compare-card.good .nt-co-eyebrow{
  background: linear-gradient(135deg, var(--nt-gold), var(--nt-gold-2));
  color: #231f15;
}

.nt-list{
  margin: 0;
  padding-left: 0;
  list-style: none;
  color: #334155;
}
.nt-list li{ display: flex; align-items: center; gap: 10px; margin: 6px 0; }

.nt-list .good{
  color: #16a34a;                   /* green check */
  font-weight: 900;
  text-decoration: none;
}
.nt-list .bad{
  color: #ef4444;                   /* red cross */
  font-weight: 900;
}
.nt-list .neutral{
  color: #94a3b8;                   /* muted dash */
  font-weight: 900;
}

.nt-compare-note{
  text-align: center;
  color: #94a3b8;
  font-size: .9rem;
  margin-top: 10px;
}

/* Gold accent for title words */
.nt-gold{
  background: linear-gradient(90deg, var(--nt-gold), var(--nt-gold-2));
  -webkit-background-clip: text;
  background-clip: text;
  color: transparent;
}

/* Right column notes grid  stacked */
.nt-notes{
  display:grid;
  gap:14px;
  margin-top:24px;
}


.nt-note{
  background:var(--nt-card);
  border:1px solid var(--nt-border);
  border-radius:var(--nt-r);
  padding:18px;
  box-shadow:var(--nt-shadow-sm);
  display:flex;
  flex-direction:column;
  gap:10px;
}
.nt-note h4{
  margin:0;
  font-size:1.05rem;
  color:var(--nt-ink);
}
.nt-note-summary{
  margin:0;
  color:var(--nt-muted);
  font-weight:500;
}
.nt-note .nt-more-btn{ align-self:flex-start; }

/* nutrition modal */
.nt-modal{
  position:fixed;
  inset:0;
  display:none;
  align-items:center;
  justify-content:center;
  z-index:1100;
}
.nt-modal.is-visible{ display:flex; }
.nt-modal__overlay{
  position:absolute;
  inset:0;
  background:rgba(15,23,42,.55);
  backdrop-filter:blur(4px);
}
.nt-modal__panel{
  position:relative;
  background:#fff;
  border-radius:22px;
  padding:28px;
  width:min(480px,90vw);
  border:1px solid var(--nt-border);
  box-shadow:0 30px 80px rgba(15,23,42,.25);
}
.nt-modal__close{
  position:absolute;
  top:14px;
  right:14px;
  width:36px;
  height:36px;
  border-radius:50%;
  border:1px solid rgba(148,163,184,.4);
  background:#f8fafc;
  color:#1f2937;
  font-size:1.4rem;
  line-height:1;
  display:grid;
  place-items:center;
  cursor:pointer;
  transition:background .2s ease, transform .2s ease, border-color .2s ease;
}
.nt-modal__close:hover,
.nt-modal__close:focus{ background:#e2e8f0; border-color:rgba(124,58,237,.4); transform:translateY(-1px); }
.nt-modal__close:focus-visible{ outline:2px solid rgba(124,58,237,.5); outline-offset:2px; }
.nt-modal__body{ margin-top:12px; }
.nt-modal__title{ margin:0 0 12px; font-size:1.35rem; color:var(--nt-ink); }
.nt-modal__content p{ margin:0 0 12px; color:#334155; line-height:1.6; }
.nt-modal__content p:last-child{ margin-bottom:0; }
.nt-modal__list{ margin:0; padding-left:20px; color:#334155; }
.nt-modal__list li{ margin-bottom:8px; }
.nt-modal__list li:last-child{ margin-bottom:0; }

@media (max-width:520px){
  .nt-modal__panel{ padding:24px 20px; border-radius:18px; }
  .nt-modal__title{ font-size:1.2rem; }
}

/* ===== HEADER / NAV ===== */
:root{
  --nav-bg-1:#0f172a;
  --nav-bg-2:#111827;
  --nav-border:rgba(255,255,255,.06);
  --nav-text:#e5e7eb;
  --nav-muted:#cbd5e1;
  --nav-purple:#8b5cf6;
  --nav-purple-2:#6366f1;
  --nav-gold:#f59e0b;
  --nav-gold-2:#fbbf24;
}

/* Container (use only if you dont already have one) */
.container {
  max-width: 1120px;
  margin-inline: auto;
  padding-inline: 16px;
}

.site-header {
  position: sticky;
  top: 0;
  z-index: 50;
  background: linear-gradient(180deg, rgba(2,6,23,.88), rgba(2,6,23,.78));
  backdrop-filter: blur(10px);
  border-bottom: 1px solid var(--nav-border);
}

.header-inner {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 8px 0;
  gap: 24px;
}

/* Remove blue link styling */
.site-header a {
  text-decoration: none;
  color: inherit;
}

/* Brand */
.brand {
  display: inline-flex;
  align-items: center;
  gap: 2px;
}

.brand-word {
  font-weight: 900;
  letter-spacing: 0.2px;
  line-height: 1;
  font-size: clamp(22px, 2.4vw, 28px);
  background: linear-gradient(90deg, #f59e0b, #fbbf24);
  -webkit-background-clip: text;
  background-clip: text;
  color: transparent;
  transition: background 0.25s ease, transform 0.2s ease;
}

.brand:hover .brand-word {
  background: linear-gradient(90deg, var(--nav-gold), var(--nav-gold-2));
  transform: translateY(-1px);
}

/* Navigation Links */
.main-nav {
  display: flex;
  align-items: center;
  gap: 32px;
}

.nav-link {
  position: relative;
  color: var(--nav-muted);
  font-weight: 700;
  letter-spacing: 0.02em;
  transition: color 0.2s ease;
}

.nav-link::after {
  content: "";
  position: absolute;
  left: 0;
  bottom: -6px;
  height: 2px;
  width: 0%;
  background: linear-gradient(90deg, var(--nav-gold), var(--nav-gold-2));
  border-radius: 2px;
  transition: width 0.22s ease;
}

.nav-link:hover {
  color: var(--nav-text);
}

.nav-link:hover::after,
.nav-link.is-active::after {
  width: 100%;
}

/* Contact Button */
.cta-contact {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  height: 40px;
  padding: 0 18px;
  border-radius: 999px;
  color: #0b1020;
  font-weight: 900;
  letter-spacing: 0.02em;
  background: linear-gradient(135deg, #f59e0b, #fbbf24);
  box-shadow: 0 8px 24px rgba(245,158,11,.35);
  transition: transform .15s ease, box-shadow .2s ease, background .25s ease;
}

.cta-contact:hover {
  background: linear-gradient(135deg, var(--nav-gold), var(--nav-gold-2));
  transform: translateY(-1px);
  box-shadow: 0 10px 28px rgba(245,158,11,.28);
}

/* Responsive */
@media (max-width: 920px) {
  .main-nav {
    gap: 18px;
  }
}

@media (max-width: 760px) {
  .main-nav {
    display: none;
  }
  .cta-contact {
    margin-left: auto;
  }
}


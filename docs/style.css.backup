  background:rgba(124,58,237,.12); color:#6d28d9; border:1px solid rgba(124,58,237,.28);
}
.sp-head h2{ font-size: clamp(1.8rem, 4vw, 2.2rem); margin:10px 0 6px; }
.sp-sub{ color:#64748b; }

/* Cards */
.sp-grid{
  display:grid; grid-template-columns: repeat(2, minmax(280px,1fr));
  gap:18px; margin-top:18px;
}
@media (max-width:900px){ .sp-grid{ grid-template-columns:1fr; } }

.sp-card{
  background:#ffffff !important;
  border:1px solid rgba(2,6,23,.08);
  border-radius:18px;
  padding:22px 22px 18px;
  box-shadow:0 12px 28px rgba(2,6,23,.08);

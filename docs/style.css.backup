}

.review-stars {
  color: #fbbf24;
  font-size: 1.2rem;
  letter-spacing: 2px;
}

.review-quote {
  font-size: 1.05rem;
  line-height: 1.7;
  color: var(--espresso);
  margin: 0;
  flex-grow: 1;
}

.review-author {
  display: flex;
  align-items: center;
  gap: 12px;
  margin-top: auto;
}

.review-avatar {

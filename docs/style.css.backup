  background:linear-gradient(90deg,#f59e0b,#fbbf24);
  -webkit-background-clip:text; background-clip:text; color:transparent;
  font-weight:900;
}
.sp-card blockquote{ margin:0 0 14px; font-size:1.05rem; font-weight:600; color:#0f172a; }

.sp-who{ display:flex; align-items:center; gap:10px; }
.sp-avatar{
  width:38px; height:38px; border-radius:50%; display:grid; place-items:center; font-weight:800; color:#fff;
  background:linear-gradient(135deg,#8b5cf6,#6366f1);
  box-shadow:0 8px 22px rgba(99,102,241,.35);
}
.sp-who small{ display:block; color:#94a3b8; margin-top:2px; }

/* Stat band */
.sp-band{
  margin-top:26px;
  background:#ffffff !important;
  border:1px solid rgba(2,6,23,.08);
  border-radius:18px;
  box-shadow:0 16px 38px rgba(2,6,23,.08);
  display:grid; grid-template-columns: repeat(4, 1fr);

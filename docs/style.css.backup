}

.story-stats .stat-item {
    text-align: center;
    padding: 16px;
    border-radius: 8px;
    background: var(--off-white);
    transition: all 0.3s ease;
}

.story-stats .stat-item:hover {
    transform: translateY(-2px);
    background: var(--primary-energy);
    color: white;
}

.story-stats .stat-item:hover .stat-label {
    color: rgba(255, 255, 255, 0.9);
}

.story-stats .stat-number {
    display: block;
    font-size: var(--font-size-2xl);
    font-weight: 800;
    color: var(--primary-energy);
    line-height: 1;
    margin-bottom: 4px;
}

.story-stats .stat-item:hover .stat-number {
    color: white;
}

.story-stats .stat-label {
    font-size: var(--font-size-xs);
    color: var(--medium-gray);
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

/* Mobile Responsive */
@media (max-width: 768px) {
    .story-grid {
        grid-template-columns: 1fr;
        gap: 40px;
    }
    
    .story-section {
        padding: 80px 0;
    }
    
    .founder-card {
        padding: 20px;
    }
    
    .founder-placeholder {
        width: 70px;
        height: 70px;
    }
    
    .founder-initials {
        font-size: var(--font-size-lg);
    }
    
    .founders-section {
        gap: 20px;
    }
    
    .story-intro h3,

/* =========================================
   SUBPAGE HEROES  coffee gradient like homepage
   ========================================= */

/* Science, Products, Nutrition, Our Story */
.science-hero,
.products-hero,
.nutrition-hero,
.story-hero {
  padding-top: 140px;  /* room for fixed navbar */
  padding-bottom: 80px;
  background:
    radial-gradient(850px 280px at 0% 0%, rgba(230, 118, 53, 0.16), transparent 60%),
    radial-gradient(850px 280px at 100% 0%, rgba(232, 221, 208, 0.7), transparent 70%),
    var(--milk-white);
  color: var(--espresso);
}

/* =========================================================
   GLOBAL TYPOGRAPHY  CLEAN SANS + READABILITY
   ========================================================= */

/* Make sure everything uses a modern sans-serif */
body {
  font-family: system-ui, -apple-system, BlinkMacSystemFont,
    "Segoe UI", "Helvetica Neue", Arial, sans-serif;
  font-size: 16px;
  line-height: 1.8;
}

/* Hero subtitles & section intros more readable */
.page-hero-sub,
.section-intro p,
.info-card p {
  font-size: 0.98rem;
  line-height: 1.8;
  color: var(--text-muted);
  max-width: 60ch;
}

/* Make main headings feel stronger + sans-serif */
h1, h2, h3,
.page-hero-title,
.section-intro h2,
.story-title {
  font-family: inherit;
  letter-spacing: -0.03em;
}

/* =========================================================
   POPPING TITLES WITH GOLDEN ENERGY ACCENT
   ========================================================= */

/* Gold accent under section titles (Science, Nutrition, etc.) */
.section-intro h2 {
  position: relative;
  display: inline-block;
  padding-bottom: 4px;
}

.section-intro h2::after {
  content: "";
  position: absolute;
  left: 0;
  bottom: 0;
  width: 52%;
  height: 3px;
  border-radius: 999px;
  background: linear-gradient(90deg, #fbbf24, #f97316);
}

/* Make hero highlight text really pop */
.page-hero-title .highlight {
  color: #fbbf24; /* golden energy */
}

/* Cards: slightly stronger headings */
.info-card h3 {
  font-size: 1.05rem;
  font-weight: 700;
  color: var(--espresso);
}

/* =========================================================
   GOLDEN ACCENTS IN NAV + ACTIVE ELEMENTS
   ========================================================= */

/* Active nav item: use golden energy colour */
.main-nav .nav-link.is-active {
  color: #fbbf24;
}

.main-nav .nav-link.is-active::after {
  width: 100%;
  background: #fbbf24;
}

/* Science/Nutrition mini pill nav: pop the active tab */
.pill-nav a.is-active {
  background: #111827;
  color: #fefce8;
  box-shadow: 0 10px 28px rgba(15, 23, 42, 0.4);
}

/* Eyebrow labels (like Lift / Smooth / Hydrate) a bit warmer */
.info-eyebrow {
  background: rgba(252, 211, 77, 0.12);
  color: #92400e;
  border: 1px solid rgba(245, 158, 11, 0.35);
}


/* ----- Home: curve-only science block ----- */
.home-curve-only .sc-header { text-align:left; margin-bottom: 0; }
.home-curve-only .sc-title { font-size: clamp(1.6rem, 3.5vw, 2.2rem); color: var(--espresso); }
.home-curve-only .sc-sub { color: var(--text-muted); max-width: 100%; margin: 8px 0 20px; }
.home-curve-only figure.curve { max-width: 100%; margin: 0; background: var(--white);
  border: 1px solid rgba(62,39,35,.08); border-radius: 16px; padding: 20px; box-shadow: var(--drip-shadow); }

@media (max-width: 768px) {
  .home-curve-only .container {
    flex-direction: column !important;
    gap: 2rem !important;
  }
  .home-curve-only .sc-header {
    text-align: center !important;
  }
  .home-curve-only .sc-sub {
    margin: 8px auto 20px !important;
  }
}

/* ----- Reviews / Our Reactions ----- */
.reviews-section { padding: var(--section-padding) 0; background: var(--milk-white); }
.reviews-header { text-align:center; margin-bottom: 40px; }
.reviews-title { font-size: clamp(1.8rem, 4vw, 2.4rem); color: var(--espresso); }
.reviews-sub { color: var(--text-muted); max-width: 680px; margin: 8px auto 0; }

.reviews-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  gap: 28px;
}

.review-card {
  background: var(--white);
  border: 1px solid rgba(62,39,35,.08);
  border-radius: 20px;
  padding: 28px;
  box-shadow: var(--drip-shadow);
  transition: transform .25s ease, box-shadow .25s ease, border-color .25s ease;
}
.review-card:hover { transform: translateY(-4px); border-color: rgba(217,121,23,.18); box-shadow: var(--float-shadow); }

.review-stars { color: #ffd166; font-weight: 700; letter-spacing: 1px; margin-bottom: 12px; }
.review-card blockquote { color: var(--espresso); font-style: italic; line-height: 1.7; margin-bottom: 18px; }

.review-meta { display: flex; align-items: center; gap: 12px; }
.review-meta .avatar {
  width: 36px; height: 36px; border-radius: 50%;
  display:flex; align-items:center; justify-content:center;
  background: linear-gradient(135deg, var(--caramel), var(--coffee-orange));
  color: var(--milk-white); font-weight: 800;
}
.review-meta .meta strong { color: var(--espresso); display:block; }
.review-meta .meta span { color: var(--text-muted); font-size: .9rem; }

.reviews-footnote { text-align:center; color: var(--text-muted); margin-top: 18px; }


/* ===== OUR REACTIONS / REVIEWS MARQUEE ===== */

.reviews-marquee {
  padding: 72px 0 64px;
  background: var(--milk-white);
}

.reviews-header {
  text-align: center;
}

.reviews-header h2 {
  font-size: clamp(1.9rem, 3.8vw, 2.4rem);
  color: var(--espresso);
  margin-bottom: 8px;
}

.reviews-sub {
  max-width: 640px;
  margin: 0 auto;
  color: var(--text-muted);
}

/* Band that holds the conveyor of cards */
.reviews-band {
  margin-top: 32px;
  overflow: hidden;
  background: #f6efe5;
  border-top: 1px solid rgba(62,39,35,0.06);
  border-bottom: 1px solid rgba(62,39,35,0.06);
  padding: 18px 0;
}

/* Track that moves left */
.reviews-track {
  display: inline-flex;
  align-items: stretch;
  gap: 20px;
  padding-left: 20px;
  animation: reviews-scroll 32s linear infinite;
}

@keyframes reviews-scroll {
  0% {
    transform: translateX(0);
  }
  100% {
    transform: translateX(-50%);
  }
}

/* Individual cards */
.review-card {
  flex: 0 0 320px;
  max-width: 360px;
  display: flex;
  align-items: flex-start;
  gap: 14px;
  padding: 18px 18px 16px;
  border-radius: 18px;
  background: var(--white);
  box-shadow: 0 10px 30px rgba(0,0,0,0.04);
  border: 1px solid rgba(62,39,35,0.06);
}

.review-avatar {
  flex-shrink: 0;
  width: 40px;
  height: 40px;
  border-radius: 999px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: radial-gradient(circle at 30% 20%, #ffe7c2, #f29b54);
  color: var(--espresso);
  font-weight: 700;
  font-size: 1rem;
}

.review-avatar-initial {
  transform: translateY(1px);
}

.review-body {
  display: flex;
  flex-direction: column;
  gap: 6px;
}

.review-stars {
  font-size: 0.85rem;
  letter-spacing: 0.08em;
  color: #fbbf24;
}

.review-text {
  margin: 0;
  font-size: 0.95rem;
  line-height: 1.6;
  color: var(--espresso);
}

.review-meta {
  margin: 0;
  font-size: 0.85rem;
  color: var(--text-muted);
}

/* Footnote */
.reviews-footnote {
  margin-top: 20px;
  text-align: center;
  color: var(--text-muted);
  font-size: 0.9rem;
}

/* Mobile tweaks */
@media (max-width: 640px) {
  .review-card {
    flex: 0 0 260px;
  }
}



/* =========================================================
   NEW REVIEWS BELT STYLING (FULLY OVERRIDES OLD)
   ========================================================= */

.reviews-gallery {
  padding: 80px 0 70px;
  background: var(--milk-white);
}

.reviews-header {
  text-align: center;
}

.reviews-header h2 {
  font-size: clamp(2rem, 3.8vw, 2.6rem);
  color: var(--espresso);
  margin-bottom: 8px;
}

.reviews-sub {
  max-width: 640px;
  margin: 0 auto;
  color: var(--text-muted);
}

/* horizontal belt */

.reviews-rail-wrapper {
  margin-top: 32px;
  overflow: hidden;
  position: relative;
  padding: 10px 0 18px;
}

/* fade edges */
.reviews-rail-wrapper::before,
.reviews-rail-wrapper::after {
  content: "";
  position: absolute;
  top: 0;
  bottom: 0;
  width: 80px;
  pointer-events: none;
  z-index: 10;
}

.reviews-rail-wrapper::before {
  left: 0;
  background: linear-gradient(to right, var(--milk-white), transparent);
}

.reviews-rail-wrapper::after {
  right: 0;
  background: linear-gradient(to left, var(--milk-white), transparent);
}

.reviews-rail {
  display: inline-flex;
  gap: 24px;
  padding-left: 24px;
  width: max-content;
  animation: reviewsRailScroll 45s ease-in-out infinite;
}

.reviews-rail-wrapper:hover .reviews-rail {
  animation-play-state: paused;
}

@keyframes reviewsRailScroll {
  0% { transform: translateX(0); }
  100% { transform: translateX(-50%); }
}

/* tiles */

.review-tile {
  flex: 0 0 260px;
  max-width: 280px;
  background: var(--white);
  border-radius: 22px;
  border: 1px solid rgba(62,39,35,0.08);
  box-shadow: 0 14px 40px rgba(15,23,42,0.06);
  overflow: hidden;
  display: flex;
  flex-direction: column;
  transition: transform 0.18s ease, box-shadow 0.18s ease, border-color 0.18s ease;
}

.review-tile:hover {
  transform: translateY(-4px);
  box-shadow: 0 18px 50px rgba(15,23,42,0.10);
  border-color: rgba(217,121,23,0.35);
}

.review-media {
  position: relative;
  width: 100%;
  aspect-ratio: 9 / 16;
  background: radial-gradient(circle at 20% 0%, #ffe7c9, #f4a65a);
  display: flex;
  align-items: center;
  justify-content: center;
}

.review-play {
  width: 56px;
  height: 56px;
}

.review-copy {
  padding: 14px 18px 16px;
}

.review-copy h3 {
  font-size: 1rem;
  margin: 0 0 4px;
  color: var(--espresso);
}

.review-copy p {
  font-size: 0.9rem;
  margin: 0;
  color: var(--text-muted);
  line-height: 1.5;
}

.reviews-footnote {
  text-align: center;
  margin-top: 20px;
  font-size: 0.9rem;
  color: var(--text-muted);
}

/* Static grid layout for 4 videos */
.reviews-static-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 40px;
  margin-top: 48px;
  max-width: 1400px;
  margin-left: auto;
  margin-right: auto;
}

.review-item {
  display: flex;
  flex-direction: column;
  gap: 16px;
}

.review-video {
  position: relative;
  width: 100%;
  aspect-ratio: 9 / 16;
  background: radial-gradient(circle at 20% 0%, #ffe7c9, #f4a65a);
  border-radius: 16px;
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
  box-shadow: 0 8px 24px rgba(15,23,42,0.08);
}

.review-video:hover {
  transform: translateY(-4px);
  box-shadow: 0 12px 32px rgba(15,23,42,0.12);
}

.review-video .review-play {
  width: 60px;
  height: 60px;
  transition: transform 0.2s ease;
  background: none;
  border: none;
}

.review-video:hover .review-play {
  transform: scale(1.1);
}

.review-text {
  text-align: center;
  padding: 0 8px;
}

.review-text h3 {
  font-size: 1.5rem;
  margin: 0 0 10px;
  color: var(--espresso);
  font-weight: 600;
}

.review-text p {
  font-size: 1.15rem;
  margin: 0;
  color: var(--text-muted);
  line-height: 1.6;
}

/* Tablet */
@media (max-width: 1024px) {
  .reviews-static-grid {
    grid-template-columns: repeat(2, 1fr);
    gap: 24px;
  }
}

/* Mobile */
@media (max-width: 640px) {
  .reviews-static-grid {
    grid-template-columns: repeat(2, 1fr);
    gap: 16px;
    max-width: 100%;
  }
  
  .review-video {
    aspect-ratio: 9 / 16;
  }
  
  .review-play {
    width: 48px;
    height: 48px;
  }
  
  .review-text {
    padding: 0 4px;
  }
  
  .review-text h3 {
    font-size: 1.1rem;
  }
  
  .review-text p {
    font-size: 0.95rem;
  }
}

/* --- Reviews belt: use scroll instead of transform animation --- */

/* stop any previous keyframe animation */
.reviews-rail {
  animation: none !important;
}

/* make the wrapper horizontally scrollable */
.reviews-rail-wrapper {
  overflow-x: auto;
  overflow-y: hidden;
  scroll-behavior: smooth;
  -webkit-overflow-scrolling: touch;
}

/* hide scrollbars (but keep scroll functionality) */
.reviews-rail-wrapper {
  scrollbar-width: none; /* Firefox */
}
.reviews-rail-wrapper::-webkit-scrollbar {
  display: none; /* Chrome / Safari */
}

/* ===== SOCIAL MEDIA SECTION ===== */

.social-section {
  padding: 80px 0;
  background: var(--white);
}

.social-header {
  text-align: center;
  margin-bottom: 48px;
}

.social-header h2 {
  font-size: clamp(1.9rem, 3.8vw, 2.4rem);
  color: var(--espresso);
  margin-bottom: 12px;
}

.social-sub {
  max-width: 640px;
  margin: 0 auto;
  color: var(--text-muted);
  font-size: 1.1rem;
}

.social-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 20px;
  max-width: 1200px;
  margin: 0 auto 40px;
}

.social-item {
  position: relative;
  aspect-ratio: 1 / 1;
  overflow: hidden;
  border-radius: 12px;
  cursor: pointer;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.social-item:hover {
  transform: translateY(-4px);
  box-shadow: 0 12px 32px rgba(15,23,42,0.12);
}

.social-image {
  width: 100%;
  height: 100%;
  background: radial-gradient(circle at 30% 20%, #ffe7c9, #f4a65a);
  display: flex;
  align-items: center;
  justify-content: center;
}

.social-cta {
  text-align: center;
  margin-top: 40px;
}

.social-link-btn {
  display: inline-block;
  padding: 1em 2em;
  background-color: rgba(255, 208, 116) !important;
  color: #1d1d1f !important;
  text-decoration: none;
  border-radius: 0px !important;
  font-weight: 600 !important;
  font-size: 17px !important;
  cursor: pointer !important;
  transition: all 0.3s ease-in-out !important;
  border: 1px solid black !important;
  box-shadow: 0 0 0 0 black !important;
  -webkit-font-smoothing: antialiased !important;
  -moz-osx-font-smoothing: grayscale !important;
  backface-visibility: hidden !important;
  transform: translateZ(0) !important;

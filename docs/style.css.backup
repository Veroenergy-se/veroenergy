/* Add LED glow effect */
.quantity-card::before {
  content: '';
  position: absolute;
  top: -2px;
  left: -2px;
  right: -2px;
  bottom: -2px;
  background: linear-gradient(45deg, #00d435, #00c230, #00d435);
  border-radius: 0px;
  z-index: -1;
  opacity: 0.7;
  filter: blur(8px);
  animation: ledGlow 2s ease-in-out infinite alternate;
}

@keyframes ledGlow {
  0% {
    opacity: 0.7;
    filter: blur(8px);
  }
  100% {
    opacity: 1;
    filter: blur(12px);
  }
}

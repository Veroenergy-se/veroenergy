.story-visual{ text-align:center; }

.polaroid-frame {
  display: inline-block;
  background-image: url(' .jpg');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
  padding: 8% 8% 15% 8%;
  position: relative;
  width: 100%;
  max-width: 600px;
}

.polaroid-frame .story-image {
  width: 100%;
  aspect-ratio: 4/3;
  object-fit: cover;
  border-radius: 0;
  box-shadow: none;
  outline: none;
  transition: transform .35s ease;
  display: block;
}

.story-image{
  width:100%; max-width:560px; aspect-ratio: 16/10; object-fit: cover;
  border-radius:22px; box-shadow:0 22px 46px rgba(0,0,0,.45);
  outline:1px solid rgba(255,255,255,.08); outline-offset:-6px;
  transition: transform .35s ease, box-shadow .35s ease;
}
.story-image:hover{ transform:scale(1.02); box-shadow:0 28px 60px rgba(0,0,0,.5); }
.img-caption{
  font-size:.85rem; color:#9aa3b2; margin-top:10px;
}

/* Founders row */
.founders-row{
  margin-top:24px;
  display:grid; grid-template-columns: repeat(3, minmax(220px,1fr));
  gap:24px;
}
.founder-card{
  background: rgba(255,255,255,.06);
  border:1px solid rgba(255,255,255,.12);
  border-radius:20px; padding:28px 22px;
  text-align:center;
  box-shadow: 0 12px 32px rgba(0,0,0,.3);
  height:100%;
  cursor: pointer;
  transition: all 0.3s ease;
}
.founder-card:hover{
  background: rgba(255,255,255,.09);
  border-color: rgba(139,92,246,.4);
  transform: translateY(-4px);
  box-shadow: 0 16px 40px rgba(0,0,0,.4);
}
.founder-avatar{
  width:110px; height:110px; border-radius:50%;
  margin:0 auto 16px;
  display:grid; place-items:center;
  background:linear-gradient(135deg,#8b5cf6,#6366f1);
  color:#fff; font-weight:800;
  box-shadow:0 10px 28px rgba(99,102,241,.45);
  transition: transform 0.3s ease;
}
.founder-card:hover .founder-avatar{
  transform: scale(1.05);
}
.founder-avatar span{ font-size:1.4rem; letter-spacing:.08em; }
.founder-card h3{ margin:8px 0 6px; font-size:1.3rem; }
.founder-card .role{ margin:0 0 12px; color:#a7b0bf; font-size:0.85rem; letter-spacing: 0.05em; text-transform: uppercase; }
.founder-card blockquote{ color:#e5e7eb; font-style:italic; margin:0; font-size:1.05rem; line-height:1.5; }

.no-scroll {
  overflow: hidden;
}

.founder-modal {
  position: fixed;
  inset: 0;
  display: none;
  align-items: center;
  justify-content: center;
  z-index: 1000;
}

.founder-modal.is-visible {
  display: flex;
}

.founder-modal__overlay {
  position: absolute;
  inset: 0;
  background: rgba(15, 23, 42, 0.65);
}

.founder-modal__dialog {
  position: relative;
  background: #0b1220;
  border: 1px solid rgba(148, 163, 184, 0.25);
  border-radius: 16px;
  padding: 32px;
  max-width: 460px;
  width: calc(100% - 32px);
  box-shadow: 0 20px 60px rgba(15, 23, 42, 0.35);
  color: #e2e8f0;
}

.founder-modal__close {
  position: absolute;
  top: 16px;
  right: 16px;
  width: 36px;
  height: 36px;
  border-radius: 50%;
  border: none;
  background: rgba(148, 163, 184, 0.12);
  color: #e2e8f0;
  font-size: 1.3rem;
  cursor: pointer;
  transition: background 0.2s ease, transform 0.2s ease;
}

.founder-modal__close:hover,
.founder-modal__close:focus {
  background: rgba(148, 163, 184, 0.25);
  transform: scale(1.05);
}

.founder-modal__content {
  display: grid;
  gap: 12px;
}

.founder-modal__name {
  margin: 0;
  font-size: 1.6rem;
  font-weight: 700;
}

.founder-modal__role {
  margin: 0;
  font-size: 0.95rem;
  letter-spacing: 0.08em;
  text-transform: uppercase;
  color: #cbd5f5;
}

.founder-modal__story {
  margin: 0;
  line-height: 1.6;
  color: #cdd5e7;
}

@media (max-width: 640px) {
  .founder-modal__dialog {
    padding: 28px 24px 32px;
  }

  .founder-modal__name {
    font-size: 1.4rem;
  }
}


/* Responsive */
@media (max-width: 1024px){
  .story-grid{ grid-template-columns: 1fr; gap:32px; }
  .story-columns{ grid-template-columns:1fr; }
  .story-image{ max-width:100%; }
}


/* Golden-orange highlight for words */
.title-line.highlight{
  color: transparent;
  background: linear-gradient(90deg, #f59e0b, #fbbf24);
  -webkit-background-clip: text; background-clip: text;
  text-shadow: 0 8px 22px rgba(245,158,11,.28);
}

/* Subtle orange hover accent for links/buttons */
a:hover { color: #ffd166; }

/* Optional tiny glow on small badges */
.section-badge, .hero-badge{
  box-shadow: 0 0 0 3px rgba(245,158,11,.12) inset;
}

/* Reusable purple band */
.band-purple{
  background: linear-gradient(180deg, #2a2145, #1f1b38);
  color: #e9e7ff;
  border-top: 1px solid rgba(255,255,255,.06);
  border-bottom: 1px solid rgba(255,255,255,.06);
}


/* Section separators (very subtle) */
section{ position: relative; }
section + section::before{
  content:""; position: absolute; top:-1px; left:0; right:0; height:1px;
  background: linear-gradient(90deg, transparent, rgba(255,255,255,.08), transparent);
}

/* ===== Full-bleed photo hero with movement ===== */
.hero.hero--photo {
  min-height: 92vh;
  display: grid;
  align-items: center;
  position: relative;
  isolation: isolate;

  /* Background image and gradient overlay */
  background-image:
    linear-gradient(180deg, rgba(20,16,40,.60) 0%, rgba(20,16,40,.78) 55%, rgba(20,16,40,.92) 100%),
    url("hero-image.jpg");
  background-size: cover;
  background-position: 55% 45%;
  background-repeat: no-repeat;

  /* Smooth parallax effect */
  background-attachment: fixed;
  animation: heroPan 22s ease-in-out infinite alternate;
}

/* Gentle background movement */
@keyframes heroPan {
  0%   { background-position: 52% 40%; }
  100% { background-position: 58% 50%; }
}

/* Subtle moving light overlay */
.hero.hero--photo::after {
  content: "";
  position: absolute;
  inset: 0;
  background:
    radial-gradient(60% 50% at 70% 30%, rgba(139,92,246,.18), transparent 60%),
    radial-gradient(40% 35% at 20% 70%, rgba(245,158,11,.10), transparent 60%);
  mix-blend-mode: screen;
  animation: sheenDrift 14s ease-in-out infinite alternate;
  pointer-events: none;
}

@keyframes sheenDrift {
  from { transform: translate3d(-2%, 1%, 0); }
  to   { transform: translate3d(2%, -1%, 0); }
}

/* Make text readable over photo */
.hero.hero--photo .hero-content {
  max-width: 720px;
  padding: 24px;
  background: linear-gradient(180deg, rgba(0,0,0,.22), rgba(0,0,0,0));
  border-radius: 18px;
}

/* Buttons pop nicely */
.hero.hero--photo .cta-primary {
  box-shadow: 0 18px 36px rgba(0,0,0,.35);
}

/* Mobile and reduced motion */
@media (max-width: 980px) {
  .hero.hero--photo { background-attachment: scroll; }
  .hero.hero--photo .hero-content {
    background: linear-gradient(180deg, rgba(0,0,0,.28), rgba(0,0,0,0));
  }
}
@media (prefers-reduced-motion: reduce) {
  .hero.hero--photo { animation: none; }
  .hero.hero--photo::after { animation: none; }
}

/* ===== SCIENCE (detailed, interactive, on-brand) ===== */
:root{
  /* safe defaults if not already set */
  --accent:#8b5cf6;          /* purple */
  --accent-2:#22c55e;        /* mint */
  --energy:#f59e0b;          /* golden highlight */
  --ink:#0f172a;             /* dark text */
  --surface:#fbfbfd;         /* light bg */
}

.science-section{
  padding: 96px 0 110px;
  background:
    radial-gradient(1200px 420px at 12% -8%, rgba(139,92,246,.10), transparent 60%),
    radial-gradient(1000px 360px at 88% -10%, rgba(245,158,11,.08), transparent 60%),
    var(--surface);
  color: var(--ink);
}

.sc-header{ text-align:center; max-width:820px; margin:0 auto 26px; }
.sc-badge{
  display:inline-block; padding:6px 12px; border-radius:999px; font-weight:700; font-size:.86rem;
  background: rgba(124,58,237,.12); color:#6d28d9; border:1px solid rgba(124,58,237,.28);
}
.sc-title{ font-size: clamp(1.9rem, 4vw, 2.5rem); margin:10px 0 6px; }
.sc-title .gold{
  background: linear-gradient(90deg, var(--energy), #fbbf24);
  -webkit-background-clip:text; background-clip:text; color:transparent;
}
.sc-sub{ color:#64748b; }

.curve{ max-width: 680px; margin: 18px auto 8px; text-align:center; }
.curve svg{ width:100%; height:auto; display:block; }
.curve figcaption{ color:#94a3b8; font-size:.86rem; margin-top:6px; }

/* Ingredient grid */
.ing-grid{
  margin-top: 26px;
  display:grid;
  grid-template-columns: repeat(4, minmax(230px,1fr));
  gap: 18px;
}
@media (max-width:1100px){ .ing-grid{ grid-template-columns: repeat(2, minmax(230px,1fr)); } }
@media (max-width:640px){ .ing-grid{ grid-template-columns: 1fr; } }

.ing-card{
  background:#fff;
  border:1px solid rgba(2,6,23,.06);
  border-radius: 18px;
  padding: 18px;
  box-shadow: 0 10px 24px rgba(2,6,23,.06);
  transition: transform .2s ease, box-shadow .2s ease, border-color .2s ease;
  will-change: transform;
}
.ing-card:hover{ transform: translateY(-4px); box-shadow: 0 14px 30px rgba(2,6,23,.08); }
.ing-card:focus{ outline:2px solid rgba(124,58,237,.35); outline-offset:3px; }

.ing-eyebrow{
  display:inline-block; padding:4px 10px; border-radius:999px; font-size:.75rem; font-weight:800; letter-spacing:.04em;
  color:#fff; margin-bottom:8px;
}
.ing-lift .ing-eyebrow{ background:linear-gradient(135deg,#8b5cf6,#6366f1); }
.ing-smooth .ing-eyebrow{ background:linear-gradient(135deg,#6366f1,#8b5cf6); }
.ing-hydrate .ing-eyebrow{ background:linear-gradient(135deg,#10b981,#22c55e); }
.ing-adapt .ing-eyebrow{ background:linear-gradient(135deg,#f59e0b,#fbbf24); color:#231f15; }

.ing-card h3{ margin:4px 0 6px; }
.ing-card p{ color:#475569; margin:0 0 10px; }
.ing-facts{ margin:0; padding-left:18px; color:#334155; }
.ing-facts li{ margin:6px 0; }
.mono{ font-variant-numeric: tabular-nums; font-family: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, "Liberation Mono", monospace; }

.ing-evidence{
  margin-top:10px; background:#f8fafc; border:1px solid rgba(2,6,23,.06);
  border-radius:12px; padding:10px 12px; color:#334155;
}
.ing-evidence summary{ cursor:pointer; font-weight:700; color:#334155; }
.ing-evidence[open]{ box-shadow: 0 10px 24px rgba(2,6,23,.06); }
.ing-evidence .note{ color:#94a3b8; }

/* How it works */
.hiw{ margin-top: 34px; }
.hiw-title{ text-align:center; margin-bottom:12px; }
.hiw-steps{
  counter-reset: step;
  margin:0 auto; max-width:980px;
  display:grid; grid-template-columns: repeat(2,1fr); gap: 14px 22px;
}
@media (max-width:820px){ .hiw-steps{ grid-template-columns:1fr; } }
.hiw-steps li{
  list-style:none; background:#fff; border:1px solid rgba(2,6,23,.06);
  border-radius:14px; padding:14px 16px; color:#334155;
  box-shadow: 0 10px 22px rgba(2,6,23,.05);
}
.hiw-steps li span{
  display:inline-grid; place-items:center; width:24px; height:24px; border-radius:50%;
  background:linear-gradient(135deg, var(--accent), #6366f1);
  color:#fff; font-weight:800; margin-right:8px; font-size:.9rem;
}

/* FAQ */
.sc-faq{ margin: 28px auto 0; max-width: 880px; display:grid; gap:10px; }
.sc-faq details{
  background:#fff; border:1px solid rgba(2,6,23,.06); border-radius:12px; padding:12px 14px; color:#334155;
}
.sc-faq summary{ cursor:pointer; font-weight:700; color:#334155; }
.sc-faq details[open]{ box-shadow: 0 10px 22px rgba(2,6,23,.06); }

/* Footnote */
.sc-foot{ color:#94a3b8; margin-top:16px; text-align:center; font-size:.9rem; }

/* ===== SOCIAL PROOF (scoped, clean) ===== */
.sp-section{
  padding: 72px 0 96px;
  background:
    radial-gradient(1200px 420px at 15% -8%, rgba(139,92,246,.10), transparent 60%),
    radial-gradient(1000px 360px at 85% -10%, rgba(245,158,11,.08), transparent 60%),

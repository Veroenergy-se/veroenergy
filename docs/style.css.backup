.hero-headline em{
  font-family: inherit;
  font-style: normal;
  font-weight: 900;
  letter-spacing: .015em;
  display: inline-block;
  transform: rotate(-1.5deg) scale(1.05);
  text-decoration: underline;
  text-decoration-thickness: .18em;
  text-underline-offset: .18em;
  text-decoration-skip-ink: none;
  text-shadow: 0 4px 12px rgba(15,23,42,.18);
}

/* Reset and Base Styles */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

html {
    scroll-behavior: smooth;
}

body {
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Helvetica Neue', Arial, sans-serif;
    line-height: 1.75;
    color: var(--espresso);
    background: var(--milk-white);
    overflow-x: hidden;
}

:root {
    /* === Global Font Reset: Sans-serif everywhere === */
html, body, * {
  font-family: system-ui, -apple-system, BlinkMacSystemFont,
    "Segoe UI", "Helvetica Neue", Arial, sans-serif !important;
}

    /* Coffee-Inspired Color Palette */
    --espresso: #3E2723;          /* Deep espresso brown */
    --caramel: #D97917;           /* Rich caramel */
    --cream: #F9F5F0;             /* Warm cream */
    --milk-white: #FFFEFB;        /* Near-white with warmth */
    --coffee-orange: #E67635;     /* Bold coffee orange */
    --latte: #E8DDD0;             /* Soft latte */
    --dark-roast: #2C1810;        /* Darkest accent */
    
    /* Liquid/Flow Accents */
    --drip-shadow: 0 8px 16px rgba(62, 39, 35, 0.15);
    --splash-shadow: 0 12px 32px rgba(217, 121, 23, 0.2);
    --float-shadow: 0 16px 40px rgba(230, 118, 53, 0.25);
    
    /* Neutrals */
    --white: #ffffff;
    --off-white: #FFFEFB;
    --light-cream: #F5F0E8;
    --text-dark: #3E2723;
    --text-muted: #6B5445;
    
    /* Spacing - Extra Breathable */
    --section-padding: 140px;
    --container-padding: 48px;
    --element-gap: 80px;
    
    /* Typography - Bold & Clean */
    --font-size-xs: 0.75rem;
    --font-size-sm: 0.875rem;
    --font-size-base: 1rem;
    --font-size-lg: 1.125rem;
    --font-size-xl: 1.375rem;
    --font-size-2xl: 1.75rem;
    --font-size-3xl: 2.5rem;
    --font-size-4xl: 3.5rem;
    --font-size-5xl: 4.5rem;
    
    /* Liquid Curves & Shapes */
    --curve-smooth: cubic-bezier(0.4, 0, 0.2, 1);
    --curve-bounce: cubic-bezier(0.68, -0.55, 0.265, 1.55);
    --border-radius-fluid: 60px;
    --border-radius-splash: 42% 58% 70% 30% / 45% 45% 55% 55%;
}

/* ===== COFFEE LIQUID SHAPES ===== */
/* Flowing drip decorative elements */
.coffee-drip {
    position: absolute;
    width: 80px;
    height: 120px;
    background: linear-gradient(180deg, var(--caramel), var(--coffee-orange));
    border-radius: 50% 50% 50% 50% / 60% 60% 40% 40%;
    opacity: 0.12;
    filter: blur(20px);
    animation: drip 8s ease-in-out infinite;
}

@keyframes drip {
    0%, 100% { transform: translateY(0) scale(1); }
    50% { transform: translateY(20px) scale(1.05); }
}

/* Coffee splash shape */
.coffee-splash {
    position: absolute;
    width: 300px;
    height: 300px;
    background: radial-gradient(circle, var(--coffee-orange) 0%, transparent 70%);
    border-radius: var(--border-radius-splash);
    opacity: 0.08;
    animation: splash-morph 20s ease-in-out infinite;
}

@keyframes splash-morph {
    0%, 100% { border-radius: 42% 58% 70% 30% / 45% 45% 55% 55%; }
    25% { border-radius: 58% 42% 50% 50% / 55% 45% 45% 55%; }
    50% { border-radius: 50% 50% 58% 42% / 45% 55% 45% 55%; }
    75% { border-radius: 42% 58% 42% 58% / 55% 45% 55% 45%; }
}

/* Flowing wave divider */
.wave-divider {
    position: relative;
    width: 100%;
    height: 120px;
    overflow: hidden;
}

.wave-divider svg {
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 120px;
}

.wave-divider path {
    fill: var(--cream);
    animation: wave-flow 15s ease-in-out infinite;
}

@keyframes wave-flow {
    0%, 100% { d: path('M0,40 Q250,60 500,40 T1000,40 V120 H0 Z'); }
    50% { d: path('M0,60 Q250,40 500,60 T1000,60 V120 H0 Z'); }
}

/* Utility Classes */
.container {
    max-width: 1320px;
    margin: 0 auto;
    padding: 0 var(--container-padding);
}

.section-badge {
    display: inline-block;
    padding: 10px 24px;
    background: linear-gradient(135deg, var(--espresso), var(--dark-roast));
    color: var(--milk-white);
    border-radius: 50px;
    font-size: var(--font-size-sm);
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1px;
    margin-bottom: 24px;
    box-shadow: var(--drip-shadow);
    position: relative;
    overflow: hidden;
}

.section-badge::before {
    content: '';
    position: absolute;
    top: -50%;
    left: -50%;
    width: 200%;
    height: 200%;
    background: radial-gradient(circle, rgba(255,255,255,0.15) 0%, transparent 70%);
    animation: badge-shimmer 6s ease-in-out infinite;
}

@keyframes badge-shimmer {
    0%, 100% { transform: translate(0, 0); }
    50% { transform: translate(20px, 20px); }
}

.section-header {
    text-align: center;
    margin-bottom: 80px;
}

.section-header h2 {
    font-size: var(--font-size-4xl);
    font-weight: 900;
    color: var(--espresso);
    margin-bottom: 32px;
    line-height: 1.1;
    letter-spacing: -0.02em;
}

.section-header p {
    font-size: var(--font-size-lg);
    color: var(--text-muted);
    max-width: 680px;
    margin: 0 auto;
    line-height: 1.8;
}

/* Introducing Banner */
.introducing-banner {
    position: relative;
    width: 100%;
    height: 80px;
    background: linear-gradient(135deg, #c4f82a 0%, #a8e617 100%);
    overflow: hidden;
    z-index: 999;
}

.introducing-marquee {
    display: flex;
    align-items: center;
    height: 100%;
    white-space: nowrap;
    animation: marqueeScroll 20s linear infinite;
}

.introducing-word {
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Helvetica Neue', Arial, sans-serif;
    font-size: clamp(2.5rem, 5vw, 4rem);
    font-weight: 900;
    color: #111827;
    text-transform: uppercase;
    letter-spacing: -0.03em;
    padding: 0 60px;
    text-shadow: 
        3px 3px 0 rgba(0, 0, 0, 0.1),
        -1px -1px 0 rgba(255, 255, 255, 0.3);
}

@keyframes marqueeScroll {
    0% {
        transform: translateX(0);
    }
    100% {
        transform: translateX(-50%);
    }
}

/* Header and Navigation */
header.site-header {
    background: rgba(17, 24, 39, 0.95);
    backdrop-filter: blur(20px);
    -webkit-backdrop-filter: blur(20px);
    color: white;
    padding: 20px 0;
    position: sticky;
    top: 0;
    width: 100%;
    z-index: 1000;
    border-bottom: 1px solid rgba(245, 158, 11, 0.2);
    box-shadow: 0 4px 24px rgba(0, 0, 0, 0.1);
}

.navbar {
    display: flex;
    justify-content: space-between;
    align-items: center;
    max-width: 1400px;
    margin: 0 auto;
    padding: 0 var(--container-padding);
    position: relative; /* ensure navbar scrolls with the page */
}

.nav-brand .logo h1 {
    font-size: 2.2rem;
    font-weight: 800;
    background: linear-gradient(135deg, #f59e0b 0%, #fbbf24 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    letter-spacing: -1px;
    margin-bottom: -4px;
}

.nav-brand .logo .tagline {
    font-size: var(--font-size-xs);
    color: rgba(255, 255, 255, 0.7);
    font-weight: 500;
    text-transform: uppercase;
    letter-spacing: 2px;
}

.nav-menu {
    display: flex;
    list-style: none;
    gap: 2.5rem;
    align-items: center;
}

.nav-menu a {
    color: rgba(255, 255, 255, 0.9);
    text-decoration: none;
    font-weight: 500;
    font-size: var(--font-size-sm);
    transition: all 0.3s ease;
    position: relative;
}

.nav-menu a::after {
    content: '';
    position: absolute;
    bottom: -4px;
    left: 0;
    width: 0;
    height: 2px;
    background: #f59e0b;
    border-radius: 2px;
    transition: width 0.3s ease;
}

.nav-menu a:hover {
    color: #fbbf24;
}

.nav-menu a:hover::after {
    width: 100%;
}

.nav-cta .nav-button {
    background: linear-gradient(135deg, #f59e0b 0%, #fbbf24 100%);
    color: white;
    border: none;
    padding: 12px 24px;
    border-radius: 25px;
    font-weight: 600;
    font-size: var(--font-size-sm);
    cursor: pointer;
    transition: all 0.3s ease;
    box-shadow: var(--shadow-md);
}

.nav-cta .nav-button:hover {
    transform: translateY(-2px);
    box-shadow: var(--shadow-lg);
    background: var(--gradient-energy);
}

/* Hero Section */
.hero {
    position: relative;
    min-height: 100vh;
    display: flex;
    align-items: center;
    overflow: hidden;
    background: var(--milk-white);
}

.hero-background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-image: 
        linear-gradient(135deg, rgba(17, 24, 39, 0.85) 0%, rgba(31, 41, 55, 0.8) 100%),
        url('https://images.unsplash.com/photo-1534438327276-14e5300c3a48?ixlib=rb-4.0.3&auto=format&fit=crop&w=2070&q=80');
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    z-index: 1;
}

.hero-video-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(45deg, 
        rgba(99, 102, 241, 0.03) 0%, 
        rgba(245, 158, 11, 0.04) 50%,
        rgba(16, 185, 129, 0.03) 100%);
}

.hero-container {
    position: relative;
    z-index: 2;
    display: flex;
    align-items: center;
    width: 100%;
    max-width: 1400px;
    margin: 0 auto;
    padding: 0 var(--container-padding);
    gap: 80px;
}

.hero-content {
    flex: 1;
    max-width: 600px;
}

.hero-badge {
    display: inline-block;
    background: var(--gradient-primary);
    color: white;
    padding: 6px 16px;
    border-radius: 20px;
    font-size: var(--font-size-xs);
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1px;
    margin-bottom: 24px;
    animation: fadeInUp 0.8s ease-out;
}

.hero-title {
    font-size: var(--font-size-5xl);
    font-weight: 800;
    line-height: 1.1;
    margin-bottom: 24px;
    text-shadow: 0 4px 20px rgba(0, 0, 0, 0.6), 0 2px 8px rgba(0, 0, 0, 0.8);
}

.title-line {
    display: block;
    color: white;
    text-shadow: 0 4px 20px rgba(0, 0, 0, 0.8), 0 2px 8px rgba(0, 0, 0, 0.9);
}

.title-line.highlight {
    background: linear-gradient(135deg, #f59e0b 0%, #fbbf24 50%, #f59e0b 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    text-shadow: none;
    filter: drop-shadow(0 4px 12px rgba(245, 158, 11, 0.5)) drop-shadow(0 2px 6px rgba(0, 0, 0, 0.8));
}

.hero-subtitle {
    font-size: var(--font-size-lg);
    color: rgba(255, 255, 255, 0.9);
    line-height: 1.7;
    margin-bottom: 40px;
    animation: fadeInUp 0.8s ease-out 0.4s both;
    text-shadow: 0 3px 12px rgba(0, 0, 0, 0.7), 0 1px 4px rgba(0, 0, 0, 0.9);
    font-weight: 400;
}

.hero-actions {
    display: flex;
    gap: 20px;
    margin-bottom: 60px;
    animation: fadeInUp 0.8s ease-out 0.6s both;
}

.cta-primary {
    background-color: rgba(255, 208, 116) !important;
    color: #1d1d1f !important;
    border-radius: 10em !important;
    font-size: 17px !important;
    font-weight: 600 !important;
    padding: 1em 2em !important;
    cursor: pointer !important;
    transition: all 0.3s ease-in-out !important;
    border: 1px solid black !important;
    box-shadow: 0 0 0 0 black !important;
    text-decoration: none !important;
    display: inline-block !important;
    text-transform: none !important;
    letter-spacing: normal !important;
    -webkit-font-smoothing: antialiased !important;
    -moz-osx-font-smoothing: grayscale !important;
    backface-visibility: hidden !important;
    transform: translateZ(0) !important;
    will-change: transform !important;
}

.cta-primary:hover {
    transform: translateY(-4px) translateX(-2px) !important;
    box-shadow: 2px 5px 0 0 black !important;
}

.cta-primary:active {
    transform: translateY(2px) translateX(1px) !important;
    box-shadow: 0 0 0 0 black !important;
}

.cta-secondary {
    background-color: white !important;
    color: black !important;
    border-radius: 10em !important;
    font-size: 17px !important;
    font-weight: 600 !important;
    padding: 1em 2em !important;
    cursor: pointer !important;
    transition: all 0.3s ease-in-out !important;
    border: 1px solid black !important;
    box-shadow: 0 0 0 0 black !important;
    text-decoration: none !important;
    display: inline-block !important;
    text-transform: none !important;
    letter-spacing: normal !important;
}

.cta-secondary:hover {
    transform: translateY(-4px) translateX(-2px) !important;
    box-shadow: 2px 5px 0 0 black !important;
}

.cta-secondary:active {
    transform: translateY(2px) translateX(1px) !important;
    box-shadow: 0 0 0 0 black !important;
}

.hero-stats {
    display: flex;
    gap: 40px;
    animation: fadeInUp 0.8s ease-out 0.8s both;
}

.hero-stats .stat {
    text-align: left;
}

.hero-stats .stat-number {
    display: block;
    font-size: var(--font-size-3xl);
    font-weight: 800;
    color: var(--primary-energy);
    line-height: 1;
    margin-bottom: 4px;
}

.hero-stats .stat-label {
    font-size: var(--font-size-sm);
    color: rgba(255, 255, 255, 0.8);
    font-weight: 500;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    text-shadow: 0 2px 8px rgba(0, 0, 0, 0.8);
}

.hero-visual {
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    animation: fadeInRight 1s ease-out 0.4s both;
}

.product-showcase {
    position: relative;
    width: 400px;
    height: 500px;
}

.product-bars {
    position: relative;
    width: 100%;
    height: 100%;
}

.protein-bar {
    position: absolute;
    width: 80px;
    height: 200px;
    border-radius: 12px;
    box-shadow: var(--shadow-xl);
    transition: all 0.4s ease;
    cursor: pointer;
}

.bar-energy {
    background: linear-gradient(135deg, var(--energy-color) 0%, var(--accent-color) 100%);
    left: 50px;
    top: 50px;
    transform: rotate(-10deg);
    animation: float 3s ease-in-out infinite;
    box-shadow: var(--shadow-lg);
}

.bar-focus {
    background: linear-gradient(135deg, var(--focus-color) 0%, var(--secondary-energy) 100%);
    right: 50px;
    top: 100px;
    transform: rotate(10deg);
    animation: float 3s ease-in-out infinite 0.5s;
    box-shadow: var(--shadow-lg);
}

.bar-recovery {
    background: linear-gradient(135deg, var(--recovery-color) 0%, var(--primary-energy) 100%);
    left: 160px;
    bottom: 50px;
    transform: rotate(-5deg);
    animation: float 3s ease-in-out infinite 1s;
    box-shadow: var(--shadow-lg);
}

.protein-bar:hover {
    transform: scale(1.1) rotate(0deg);
    z-index: 10;
}

.floating-elements {
    position: absolute;
    width: 100%;
    height: 100%;
    pointer-events: none;
}

/* Additional floating elements */
.element-4 {
    position: absolute;
    top: 15%;
    right: 10%;
    animation: pulse 3s infinite;
    font-size: 2rem;
    opacity: 0.6;
}

.element-5 {
    position: absolute;
    bottom: 25%;
    left: 15%;
    animation: bounce 4s infinite;
    font-size: 1.8rem;
    opacity: 0.5;
}

.element-6 {
    position: absolute;
    top: 60%;
    right: 20%;
    animation: rotate 5s linear infinite;
    font-size: 1.5rem;
    opacity: 0.4;
}

/* Gym background elements */
.gym-background {
    position: absolute;
    width: 100%;
    height: 100%;
    pointer-events: none;
    opacity: 0.02;
    z-index: 0;
}

.barbell-icon,
.dumbbell-icon,
.kettlebell-icon {
    position: absolute;
    width: 60px;
    height: 20px;
    background: var(--medium-gray);
    border-radius: 10px;
}

.barbell-icon {
    top: 20%;
    left: 5%;
    width: 100px;
    transform: rotate(25deg);
}

.barbell-icon::before,
.barbell-icon::after {
    content: '';
    position: absolute;
    width: 20px;
    height: 40px;
    background: var(--medium-gray);
    border-radius: 5px;
    top: -10px;
}

.barbell-icon::before {
    left: -5px;
}

.barbell-icon::after {
    right: -5px;
}

.dumbbell-icon {
    bottom: 30%;
    right: 8%;
    width: 80px;
    transform: rotate(-15deg);
}

.dumbbell-icon::before,
.dumbbell-icon::after {
    content: '';
    position: absolute;
    width: 15px;
    height: 30px;
    background: var(--medium-gray);
    border-radius: 3px;
    top: -5px;
}

.dumbbell-icon::before {
    left: -3px;
}

.dumbbell-icon::after {
    right: -3px;
}

.kettlebell-icon {
    top: 70%;
    left: 70%;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    transform: rotate(45deg);
}

.kettlebell-icon::before {
    content: '';
    position: absolute;
    width: 15px;
    height: 15px;
    background: var(--medium-gray);
    border-radius: 3px;
    top: -8px;
    left: 50%;
    transform: translateX(-50%);
}

@keyframes pulse {
    0%, 100% { transform: scale(1); opacity: 0.6; }
    50% { transform: scale(1.1); opacity: 0.8; }
}

@keyframes bounce {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-10px); }
}

@keyframes rotate {
    from { transform: rotate(0deg); }
    to { transform: rotate(360deg); }
}

.element {
    position: absolute;
    font-size: 2rem;
    opacity: 0.6;
    animation: floatElement 4s ease-in-out infinite;
}

.element-1 {
    top: 20%;
    left: 10%;
    animation-delay: 0s;
}

.element-2 {
    top: 60%;
    right: 20%;
    animation-delay: 1s;
}

.element-3 {
    bottom: 20%;
    left: 20%;
    animation-delay: 2s;
}

/* Science Section */
.science-section {
    padding: var(--section-padding) 0;
    background: var(--milk-white);
    position: relative;
    overflow: hidden;
}

.science-section::before {
    content: '';
    position: absolute;
    top: -60px;
    left: 10%;
    width: 400px;
    height: 400px;
    background: radial-gradient(circle, var(--caramel) 0%, transparent 70%);
    opacity: 0.06;
    border-radius: var(--border-radius-splash);
    animation: float-gentle 20s ease-in-out infinite;
}

@keyframes float-gentle {
    0%, 100% { transform: translate(0, 0) rotate(0deg); }
    50% { transform: translate(30px, -20px) rotate(5deg); }
}

.science-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    gap: 40px;
    margin-top: 60px;
}

.science-card {
    background: var(--white);
    border: 1px solid rgba(62, 39, 35, 0.08);
    border-radius: 28px;
    padding: 48px;
    text-align: center;
    transition: all 0.5s var(--curve-smooth);
    position: relative;
    overflow: hidden;
    box-shadow: var(--drip-shadow);
}

.science-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: linear-gradient(90deg, var(--caramel), var(--coffee-orange));
    transform: scaleX(0);
    transition: transform 0.5s var(--curve-smooth);
    border-radius: 28px 28px 0 0;
}

.science-card:hover::before {
    transform: scaleX(1);
}

.science-card.featured {
    border-color: var(--primary-gold);
    box-shadow: var(--shadow-lg);
    transform: translateY(-10px);
}

.science-card:hover {
    transform: translateY(-16px) scale(1.02);
    box-shadow: var(--float-shadow);
    border-color: rgba(217, 121, 23, 0.15);
}

.card-icon {
    font-size: 3rem;
    margin-bottom: 24px;
    display: block;
}

.science-card h3 {
    font-size: var(--font-size-xl);
    font-weight: 800;
    color: var(--espresso);
    margin-bottom: 20px;
    letter-spacing: -0.01em;
}

.science-card p {
    color: var(--text-muted);
    line-height: 1.8;
    margin-bottom: 28px;
}

.card-metric {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 12px;
    padding: 16px;
    background: var(--light-gray);
    border-radius: 12px;
    margin-top: 24px;
}

.metric-value {
    font-size: var(--font-size-2xl);
    font-weight: 800;
    color: var(--primary-gold);
}

.metric-label {
    font-size: var(--font-size-sm);
    color: var(--medium-gray);
    font-weight: 500;
}

/* Products Section */
.products-section {
    padding: var(--section-padding) 0;
    background: var(--cream);
    position: relative;
    overflow: hidden;
}

/* Add coffee splash decorations to products */
.products-section::before {
    content: '';
    position: absolute;
    top: 10%;
    right: 5%;
    width: 500px;
    height: 500px;
    background: radial-gradient(circle, var(--coffee-orange) 0%, transparent 70%);
    opacity: 0.05;
    border-radius: var(--border-radius-splash);
    animation: splash-morph 25s ease-in-out infinite;
}

.products-section::after {
    content: '';
    position: absolute;
    bottom: 15%;
    left: 8%;
    width: 350px;
    height: 350px;
    background: radial-gradient(circle, var(--caramel) 0%, transparent 70%);
    opacity: 0.06;
    border-radius: var(--border-radius-splash);
    animation: splash-morph 20s ease-in-out infinite reverse;
}

.products-carousel {
    display: flex;
    gap: 40px;
    justify-content: center;
    align-items: stretch;
    margin-top: 60px;
    flex-wrap: wrap;
}

.product-card {
    background: var(--white);
    border-radius: 32px;
    overflow: hidden;
    width: 380px;
    transition: all 0.5s var(--curve-smooth);
    position: relative;
    box-shadow: var(--drip-shadow);
    border: 1px solid rgba(62, 39, 35, 0.06);
}

.product-card.active {
    transform: scale(1.05);
    box-shadow: var(--shadow-xl);
}

.product-card:hover {
    transform: translateY(-12px) scale(1.03);
    box-shadow: var(--float-shadow);
    border-color: rgba(217, 121, 23, 0.2);
}

.product-visual {
    position: relative;
    height: 300px;
    background: linear-gradient(135deg, var(--cream), var(--latte));
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
    border-bottom: 1px solid rgba(62, 39, 35, 0.06);
}

.product-badge {
    position: absolute;
    top: 20px;
    right: 20px;
    background: linear-gradient(135deg, var(--caramel), var(--coffee-orange));
    color: var(--milk-white);
    padding: 8px 16px;
    border-radius: 24px;
    font-size: var(--font-size-xs);
    font-weight: 800;
    text-transform: uppercase;
    z-index: 2;
    box-shadow: var(--drip-shadow);
    letter-spacing: 0.5px;
}

/* 3D Protein Bars */
.bar-3d {
    width: 120px;
    height: 240px;
    border-radius: 16px;
    position: relative;
    box-shadow: var(--shadow-lg);
    transition: all 0.4s ease;
    transform: rotateY(-15deg) rotateX(10deg);
    transform-style: preserve-3d;
}

.bar-energy-3d {
    background: linear-gradient(135deg, var(--energy-color) 0%, var(--accent-color) 100%);
    box-shadow: var(--shadow-xl);
}

.bar-focus-3d {
    background: linear-gradient(135deg, var(--focus-color) 0%, var(--secondary-energy) 100%);
    box-shadow: var(--shadow-xl);
}

.bar-recovery-3d {
    background: linear-gradient(135deg, var(--recovery-color) 0%, var(--primary-energy) 100%);
    box-shadow: var(--shadow-xl);
}

.bar-3d::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(45deg, rgba(255,255,255,0.1), transparent, rgba(0,0,0,0.1));
    border-radius: 16px;
    pointer-events: none;
}

.product-card:hover .bar-3d {
    transform: rotateY(0deg) rotateX(0deg) scale(1.1);
}

.product-info {
    padding: 32px;
}

.product-info h3 {
    font-size: var(--font-size-2xl);
    font-weight: 900;
    color: var(--espresso);
    margin-bottom: 12px;
    letter-spacing: -0.02em;
}

.product-tagline {
    font-size: var(--font-size-sm);
    color: var(--caramel);
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1px;
    margin-bottom: 16px;
}

.product-description {
    color: var(--text-muted);
    line-height: 1.8;
    margin-bottom: 28px;
}

.product-specs {
    display: flex;
    flex-wrap: wrap;
    gap: 8px;
    margin-bottom: 20px;
}

.spec {
    background: var(--latte);
    color: var(--espresso);
    padding: 8px 16px;
    border-radius: 24px;
    font-size: var(--font-size-xs);
    font-weight: 700;
    border: 1px solid rgba(62, 39, 35, 0.1);
}

.flavors {
    display: flex;
    gap: 12px;
    margin-top: 20px;
}

.flavor-dot {
    width: 24px;
    height: 24px;
    border-radius: 50%;
    border: 2px solid var(--white);
    box-shadow: var(--shadow-sm);
}

.flavor-dot.coffee { background: #8b4513; }
.flavor-dot.lemon { background: #ffd700; }
.flavor-dot.blueberry { background: #4169e1; }
.flavor-dot.matcha { background: #90ee90; }
.flavor-dot.espresso { background: #3c2415; }
.flavor-dot.citrus { background: #ffa500; }
.flavor-dot.peanut { background: #daa520; }
.flavor-dot.chocolate { background: #8b4513; }
.flavor-dot.cinnamon { background: #cd853f; }

.carousel-controls {
    display: flex;
    justify-content: center;
    gap: 16px;
    margin-top: 40px;
}

.carousel-btn {
    width: 48px;
    height: 48px;
    border: 2px solid var(--primary-energy);
    background: transparent;
    color: var(--primary-energy);
    border-radius: 50%;
    font-size: var(--font-size-lg);
    cursor: pointer;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
}

.carousel-btn:hover {
    background: var(--primary-energy);
    color: white;
    transform: scale(1.1);
    box-shadow: var(--shadow-md);
}

/* Testimonials Section */
.testimonials-section {
    padding: var(--section-padding) 0;
    background: #FFFEFB;
    position: relative;
    overflow: hidden;
}

/* Add subtle coffee ripples */
.testimonials-section::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 800px;
    height: 800px;
    background: radial-gradient(circle, var(--caramel) 0%, transparent 60%);
    opacity: 0.03;
    border-radius: 50%;
    animation: ripple 30s ease-in-out infinite;
}

@keyframes ripple {
    0%, 100% { transform: translate(-50%, -50%) scale(1); }
    50% { transform: translate(-50%, -50%) scale(1.1); }
}

.testimonials-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    gap: 40px;
    margin-top: 60px;
    margin-bottom: 80px;
}

.testimonial-card {
    background: #ffffff;
    padding: 48px;
    border-radius: 32px;
    position: relative;
    transition: all 0.5s var(--curve-smooth);
    border: 1px solid rgba(62, 39, 35, 0.08);
    box-shadow: var(--drip-shadow);
}

.testimonial-card.featured {
    background: #ffffff;
    border-color: var(--primary-gold);
    box-shadow: var(--shadow-lg);
    transform: translateY(-10px);
}

.testimonial-card:hover {
    transform: translateY(-12px) scale(1.02);
    box-shadow: var(--float-shadow);
    border-color: rgba(217, 121, 23, 0.15);
}

.testimonial-rating {
    margin-bottom: 24px;
}

.testimonial-rating .stars {
    color: #ffd700;
    font-size: var(--font-size-lg);
}

.testimonial-card blockquote {
    font-size: var(--font-size-lg);
    line-height: 1.8;
    color: var(--espresso);
    margin: 0 0 28px 0;
    font-style: italic;
    position: relative;
    font-weight: 500;
}

.testimonial-card blockquote::before {
    content: '"';
    position: absolute;
    left: -20px;
    top: -12px;
    font-size: 4rem;
    color: var(--caramel);
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Helvetica Neue', Arial, sans-serif;
    opacity: 0.3;
}

.testimonial-author {
    display: flex;
    align-items: center;
    gap: 16px;
}

.author-info strong {
    display: block;
    font-weight: 600;
    color: var(--primary-dark);
    margin-bottom: 4px;
}

.author-info span {
    font-size: var(--font-size-sm);
    color: var(--medium-gray);
}

.company-stats {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 40px;
    margin-top: 60px;
    padding: 60px 40px;
    background: var(--gradient-light);
    border-radius: 24px;
    text-align: center;
}

.stat-item {
    padding: 20px;
    transition: transform 0.3s ease;
}

.stat-item:hover {
    transform: translateY(-5px);
}

.company-stats .stat-number {
    display: block;
    font-size: var(--font-size-4xl);
    font-weight: 800;
    color: var(--primary-energy);
    margin-bottom: 8px;
    line-height: 1;
}

.company-stats .stat-label {
    font-size: var(--font-size-sm);
    color: var(--medium-gray);
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

/* Footer */
footer {
    background: linear-gradient(135deg, #0f2419, #081510);
    color: var(--milk-white);
    padding: 100px 0 50px;
    position: relative;
    overflow: hidden;
}

footer::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 2px;
    background: linear-gradient(90deg, transparent, var(--caramel), transparent);
}

/* Add coffee steam effect to footer */
footer::after {
    content: '';
    position: absolute;
    top: -100px;
    left: 20%;
    width: 200px;
    height: 400px;
    background: linear-gradient(180deg, transparent, var(--caramel));
    opacity: 0.05;
    filter: blur(60px);
    animation: steam 20s ease-in-out infinite;
}

@keyframes steam {
    0%, 100% { transform: translateY(0) scaleY(1); }
    50% { transform: translateY(-40px) scaleY(1.2); }
}

.footer-content {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 60px;
    margin-bottom: 60px;
}

.footer-section h3 {
    color: var(--coffee-orange);
    margin-bottom: 28px;
    font-size: var(--font-size-xl);
    font-weight: 800;
    letter-spacing: -0.01em;
}

.footer-section p {
    margin-bottom: 12px;
    color: rgba(255, 255, 255, 0.8);
    line-height: 1.6;
}

.social-links {
    display: flex;
    gap: 16px;
    margin-top: 24px;
}

.social-links a {
    color: rgba(255, 255, 255, 0.7);
    text-decoration: none;
    padding: 12px 16px;
    border: 1px solid rgba(255, 255, 255, 0.2);
    border-radius: 8px;
    transition: all 0.3s ease;
    font-weight: 500;
}

.social-links a:hover {
    color: var(--primary-energy);
    border-color: var(--primary-energy);
    background: rgba(99, 102, 241, 0.1);
    transform: translateY(-2px);
    box-shadow: var(--shadow-sm);
}

.footer-bottom {
    border-top: 1px solid rgba(255, 255, 255, 0.1);
    padding-top: 40px;
    text-align: center;
    color: rgba(255, 255, 255, 0.6);
    font-size: var(--font-size-sm);
}

/* Animations */
@keyframes fadeInUp {
    from {
        opacity: 0;
        transform: translateY(30px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

@keyframes fadeInRight {
    from {
        opacity: 0;
        transform: translateX(30px);
    }
    to {
        opacity: 1;
        transform: translateX(0);
    }
}

@keyframes float {
    0%, 100% {
        transform: translateY(0px) rotate(-10deg);
    }
    50% {
        transform: translateY(-20px) rotate(-10deg);
    }
}

@keyframes floatElement {
    0%, 100% {
        transform: translateY(0px) scale(1);
        opacity: 0.6;
    }
    50% {
        transform: translateY(-15px) scale(1.1);
        opacity: 0.8;
    }
}

/* Electric Energy Animations */
@keyframes electricPulse {
    0%, 100% {
        box-shadow: 0 0 20px rgba(0, 212, 255, 0.4);
        transform: scale(1);
    }
    50% {
        box-shadow: 0 0 35px rgba(0, 212, 255, 0.8), 0 0 50px rgba(255, 51, 102, 0.4);
        transform: scale(1.02);
    }
}

@keyframes electricBuzz {
    0%, 100% {
        transform: translateX(0) translateY(0);
    }
    25% {
        transform: translateX(-1px) translateY(1px);
    }
    50% {
        transform: translateX(1px) translateY(-1px);
    }
    75% {
        transform: translateX(-1px) translateY(-1px);
    }
}

@keyframes energyPulse {
    0%, 100% {
        background: radial-gradient(ellipse at center, rgba(0, 212, 255, 0.15) 0%, rgba(255, 51, 102, 0.1) 50%, transparent 70%);
    }
    33% {
        background: radial-gradient(ellipse at center, rgba(255, 51, 102, 0.2) 0%, rgba(0, 255, 170, 0.15) 50%, transparent 70%);
    }
    66% {
        background: radial-gradient(ellipse at center, rgba(0, 255, 170, 0.18) 0%, rgba(255, 255, 0, 0.12) 50%, transparent 70%);
    }
}

@keyframes colorShift {
    0%, 100% {
        background: linear-gradient(45deg, 
            rgba(0, 212, 255, 0.05) 0%, 
            rgba(255, 51, 102, 0.08) 30%,
            rgba(0, 255, 170, 0.05) 60%,
            rgba(255, 255, 0, 0.03) 100%);
    }
    25% {
        background: linear-gradient(45deg, 
            rgba(255, 51, 102, 0.08) 0%, 
            rgba(0, 255, 170, 0.1) 30%,
            rgba(255, 255, 0, 0.06) 60%,
            rgba(0, 212, 255, 0.04) 100%);
    }
    50% {
        background: linear-gradient(45deg, 
            rgba(0, 255, 170, 0.1) 0%, 
            rgba(255, 255, 0, 0.08) 30%,
            rgba(255, 0, 128, 0.06) 60%,
            rgba(0, 212, 255, 0.05) 100%);
    }
    75% {
        background: linear-gradient(45deg, 
            rgba(255, 255, 0, 0.08) 0%, 
            rgba(255, 0, 128, 0.1) 30%,
            rgba(0, 212, 255, 0.07) 60%,
            rgba(255, 51, 102, 0.05) 100%);
    }
}

@keyframes textElectric {
    0%, 100% {
        filter: drop-shadow(0 0 20px rgba(0, 212, 255, 0.5));
    }
    25% {
        filter: drop-shadow(0 0 25px rgba(255, 51, 102, 0.7));
    }
    50% {
        filter: drop-shadow(0 0 30px rgba(0, 255, 170, 0.6));
    }
    75% {
        filter: drop-shadow(0 0 25px rgba(255, 255, 0, 0.5));
    }
}

@keyframes numberPulse {
    0%, 100% {
        transform: scale(1);
        filter: drop-shadow(0 0 10px rgba(0, 212, 255, 0.5));
    }
    50% {
        transform: scale(1.05);
        filter: drop-shadow(0 0 20px rgba(255, 51, 102, 0.8));
    }
}

@keyframes electricGlow {
    0%, 100% {
        box-shadow: 0 0 30px rgba(255, 102, 0, 0.6);
    }
    33% {
        box-shadow: 0 0 40px rgba(0, 255, 136, 0.8);
    }
    66% {
        box-shadow: 0 0 35px rgba(139, 92, 246, 0.7);
    }
}

@keyframes badgePulse {
    0%, 100% {
        box-shadow: 0 0 15px rgba(0, 212, 255, 0.4);
        transform: scale(1);
    }
    50% {
        box-shadow: 0 0 25px rgba(255, 51, 102, 0.6);
        transform: scale(1.03);
    }
}

@keyframes energyBarGlow {
    0%, 100% {
        box-shadow: 0 0 40px rgba(255, 102, 0, 0.7);
    }
    50% {
        box-shadow: 0 0 60px rgba(255, 255, 0, 0.9), 0 0 80px rgba(255, 0, 128, 0.5);
    }
}

@keyframes focusBarGlow {
    0%, 100% {
        box-shadow: 0 0 40px rgba(0, 255, 136, 0.7);
    }
    50% {
        box-shadow: 0 0 60px rgba(0, 212, 255, 0.9), 0 0 80px rgba(0, 255, 170, 0.5);
    }
}

@keyframes recoveryBarGlow {
    0%, 100% {
        box-shadow: 0 0 40px rgba(139, 92, 246, 0.7);
    }
    50% {
        box-shadow: 0 0 60px rgba(255, 51, 102, 0.9), 0 0 80px rgba(255, 0, 128, 0.5);
    }
}

@keyframes taglineShimmer {
    0%, 100% {
        background: var(--gradient-electric);
        -webkit-background-clip: text;
        background-clip: text;
    }
    50% {
        background: linear-gradient(45deg, #ff3366 0%, #00ffaa 100%);
        -webkit-background-clip: text;
        background-clip: text;
    }
}

@keyframes buttonBuzz {
    0%, 100% {
        transform: scale(1.15) rotate(0deg);
    }
    25% {
        transform: scale(1.18) rotate(1deg);
    }
    50% {
        transform: scale(1.15) rotate(0deg);
    }
    75% {
        transform: scale(1.18) rotate(-1deg);
    }
}

@keyframes statGlow {
    0%, 100% {
        filter: drop-shadow(0 0 15px rgba(0, 212, 255, 0.5));
    }
    50% {
        filter: drop-shadow(0 0 25px rgba(255, 51, 102, 0.8));
    }
}

@keyframes footerTitleGlow {
    0%, 100% {
        filter: drop-shadow(0 0 10px rgba(0, 212, 255, 0.4));
    }
    50% {
        filter: drop-shadow(0 0 20px rgba(0, 255, 170, 0.6));
    }
}

/* Responsive Design */
@media (max-width: 1024px) {
    :root {
        --section-padding: 80px;
        --container-padding: 24px;
        --font-size-5xl: 3rem;
        --font-size-4xl: 2.25rem;
    }
    
    .hero-container {
        gap: 60px;
    }
    
    .products-carousel {
        gap: 24px;
    }
    
    .product-card {
        width: 340px;
    }
}

@media (max-width: 768px) {
    :root {
        --section-padding: 60px;
        --container-padding: 20px;
        --font-size-5xl: 2.2rem;
        --font-size-4xl: 1.8rem;
        --font-size-3xl: 1.6rem;
    }
    
    .navbar {
        flex-direction: column;
        gap: 16px;
        padding: 12px var(--container-padding);
    }
    
    .nav-menu {
        flex-wrap: wrap;
        justify-content: center;
        gap: 1.2rem;
    }
    
    .nav-link {
        font-size: 0.9rem;
    }
    
    .nav-cta {
        order: -1;
        padding: 10px 24px;
        font-size: 0.9rem;
    }
    
    .hero-container {
        flex-direction: column;
        text-align: center;
        gap: 32px;
        padding-top: 200px;
        padding-bottom: 60px;
    }
    
    .hero-content {
        max-width: 100%;
        padding: 0 16px;
    }
    
    .hero-title {
        font-size: 2.2rem;
        line-height: 1.2;
        margin-bottom: 20px;
    }
    
    .hero-subtitle {
        font-size: 1.05rem;
        line-height: 1.6;
        margin-bottom: 32px;
    }
    
    .hero-actions {
        justify-content: center;
        flex-wrap: wrap;
        gap: 12px;
    }
    
    .cta-primary, .cta-secondary {
        padding: 14px 28px;
        font-size: 0.95rem;
    }
    
    .hero-stats {
        justify-content: center;
        flex-wrap: wrap;
        gap: 20px;
        margin-top: 40px;
    }
    
    .product-showcase {
        width: 300px;
        height: 400px;
    }
    
    .products-carousel {
        flex-direction: column;
        align-items: center;
    }
    
    .product-card {
        width: 100%;
        max-width: 400px;
    }
    
    .science-grid,
    .testimonials-grid {
        grid-template-columns: 1fr;
        gap: 24px;
    }
    
    .company-stats {
        grid-template-columns: repeat(2, 1fr);
        padding: 40px 20px;
    }
    
    .footer-content {
        grid-template-columns: 1fr;
        gap: 40px;
        text-align: center;
    }
}

@media (max-width: 480px) {
    :root {
        --section-padding: 40px;
        --container-padding: 16px;
        --font-size-5xl: 2rem;
        --font-size-4xl: 1.75rem;
        --font-size-3xl: 1.5rem;
        --font-size-2xl: 1.25rem;
    }
    
    .hero-badge {
        font-size: 0.7rem;
        padding: 4px 12px;
    }
    
    .hero-actions {
        flex-direction: column;
        align-items: center;
    }
    
    .cta-primary,
    .cta-secondary {
        width: 100%;
        max-width: 280px;
        padding: 14px 24px;
    }
    
    .hero-stats {
        grid-template-columns: 1fr;
        text-align: center;
    }
    
    .product-showcase {
        width: 280px;
        height: 350px;
    }
    
    .protein-bar {
        width: 60px;
        height: 160px;
    }
    
    .bar-energy {
        left: 40px;
        top: 40px;
    }
    
    .bar-focus {
        right: 40px;
        top: 80px;
    }
    
    .bar-recovery {
        left: 120px;
        bottom: 40px;
    }
    
    .section-header {
        margin-bottom: 40px;
    }
    
    .section-header h2 {
        font-size: var(--font-size-3xl);
    }
    
    .science-card,
    .testimonial-card {
        padding: 24px;
    }
    
    .company-stats {
        grid-template-columns: 1fr;
        padding: 24px 16px;
    }
    
    .testimonial-card blockquote {
        font-size: var(--font-size-base);
    }
}
/* Coming Soon Reviews Section */
.coming-soon-reviews {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    gap: 40px;
    margin-top: 60px;
    margin-bottom: 80px;
}

.review-placeholder {
    background: var(--light-gray);
    padding: 40px;
    border-radius: 20px;
    text-align: center;
    transition: all 0.4s ease;
    border: 2px solid transparent;
}

.review-placeholder:hover {
    transform: translateY(-5px);
    box-shadow: var(--shadow-lg);
    border-color: var(--primary-energy);
}

.placeholder-icon {
    font-size: 3rem;
    margin-bottom: 24px;
    display: block;
}

.review-placeholder h3 {
    font-size: var(--font-size-xl);
    font-weight: 700;
    color: var(--primary-dark);
    margin-bottom: 16px;
}

.review-placeholder p {
    color: var(--medium-gray);
    line-height: 1.7;
}

.launch-stats {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 40px;
    margin-top: 60px;
    padding: 60px 40px;
    background: var(--gradient-light);
    border-radius: 24px;
    text-align: center;
}

.launch-stats .stat-item {
    padding: 20px;
    transition: transform 0.3s ease;
}

.launch-stats .stat-item:hover {
    transform: translateY(-5px);
}

.launch-stats .stat-number {
    display: block;
    font-size: var(--font-size-4xl);
    font-weight: 800;
    color: var(--primary-energy);
    margin-bottom: 8px;
    line-height: 1;
}

.launch-stats .stat-label {
    font-size: var(--font-size-sm);
    color: var(--medium-gray);
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

/* Responsive adjustments */
@media (max-width: 768px) {
    .coming-soon-reviews {
        grid-template-columns: 1fr;
        gap: 24px;
    }
    
    .launch-stats {
        grid-template-columns: repeat(2, 1fr);
        padding: 40px 20px;
    }
    
    .review-placeholder {
        padding: 24px;
    }
}

@media (max-width: 480px) {
    .launch-stats {
        grid-template-columns: 1fr;
        padding: 24px 16px;

/* Nutrition Toggle Styles */
.nutrition-toggle {
    width: 100%;
    background: var(--gradient-primary);
    color: white;
    border: none;
    padding: 16px 24px;
    border-radius: 12px;
    font-size: var(--font-size-base);
    font-weight: 600;
    cursor: pointer;
    transition: all 0.3s ease;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 16px 0;
    box-shadow: var(--shadow-sm);
}

.nutrition-toggle:hover {
    transform: translateY(-2px);
    box-shadow: var(--shadow-md);
    background: linear-gradient(135deg, #5555eb 0%, #7c3aed 100%);
}

.nutrition-toggle:active {
    transform: translateY(0);
}

.toggle-icon {
    font-size: 1.2rem;
    transition: transform 0.3s ease;
}

.nutrition-facts {
    background: var(--white);
    border: 1px solid var(--light-gray);
    border-radius: 12px;
    padding: 24px;
    margin: 16px 0;
    box-shadow: var(--shadow-sm);
}

.nutrition-facts h4 {
    color: var(--primary-dark);
    font-size: var(--font-size-lg);
    font-weight: 700;
    margin-bottom: 16px;
    border-bottom: 2px solid var(--primary-energy);
    padding-bottom: 8px;
}

.nutrition-facts table {
    width: 100%;
    border-collapse: collapse;
    margin: 20px 0;
}

.nutrition-facts table td {
    padding: 8px 12px;
    border-bottom: 1px solid var(--light-gray);
    font-size: var(--font-size-sm);
}

.nutrition-facts table td:first-child {
    font-weight: 600;
    color: var(--primary-dark);
    width: 50%;
}

.nutrition-facts table td:last-child {
    color: var(--medium-gray);
    text-align: right;
    font-weight: 500;
}

.nutrition-facts p {
    font-size: var(--font-size-sm);
    line-height: 1.6;
    color: var(--dark-gray);
    margin: 16px 0;
}

.nutrition-facts p strong {
    color: var(--primary-dark);
}

/* Nutrition Card Layout */
.nutrition-card {
    background: var(--off-white);
    border-radius: 16px;
    padding: 32px;
    box-shadow: var(--shadow-sm);
    transition: all 0.3s ease;
}

.nutrition-card:hover {
    transform: translateY(-5px);
    box-shadow: var(--shadow-lg);
}

.nutrition-card h3 {
    font-size: var(--font-size-2xl);
    font-weight: 700;
    color: var(--primary-dark);
    margin-bottom: 16px;
    text-align: center;
}

.nutrition-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    gap: 40px;
    margin-bottom: 60px;
}

.regulatory-info {
    background: var(--gradient-light);
    border-radius: 16px;
    padding: 40px;
    margin-top: 40px;
    border-left: 4px solid var(--primary-energy);
}

.regulatory-info h3 {
    font-size: var(--font-size-xl);
    font-weight: 700;
    color: var(--primary-dark);
    margin-bottom: 24px;
}

.regulatory-info .warnings p {
    font-size: var(--font-size-sm);
    line-height: 1.6;
    color: var(--dark-gray);
    margin: 12px 0;
}

.caffeine-warning {
    background: rgba(239, 68, 68, 0.1);
    border: 1px solid rgba(239, 68, 68, 0.2);
    border-radius: 8px;
    padding: 16px;
    margin: 16px 0;
}

.caffeine-warning strong {
    color: var(--accent-color);
}

/* Enhanced Nutrition Panel Styling */
.nutrition-facts h5 {
    color: var(--primary-dark);
    font-size: var(--font-size-base);
    font-weight: 700;
    margin: 16px 0 12px 0;
    padding-top: 16px;
}

.nutrition-facts hr {
    border: none;
    border-top: 1px solid var(--light-gray);
    margin: 20px 0;
}

/* Our Story Section */
.story-section {
    padding: var(--section-padding) 0;
    background: linear-gradient(135deg, #fefefe 0%, #f8fafc 50%, #f1f5f9 100%);
    position: relative;
    overflow: hidden;
}


.story-content {
    max-width: 1200px;
    margin: 0 auto;
}

.story-grid {
    display: grid;
    grid-template-columns: 1fr 400px;
    gap: 80px;
    align-items: start;
}

.story-text {
    max-width: none;
}

.story-intro,
.story-journey {
    margin-bottom: 40px;
}

.story-intro h3,
.story-journey h3 {
    font-size: var(--font-size-2xl);
    font-weight: 700;
    color: var(--primary-dark);
    margin-bottom: 20px;
    line-height: 1.3;
}

.story-intro p,
.story-journey p {
    font-size: var(--font-size-lg);
    line-height: 1.7;
    color: var(--dark-gray);
    margin-bottom: 24px;
}

.story-values {
    display: grid;
    grid-template-columns: 1fr;
    gap: 24px;
    margin-top: 40px;
}

.value-item {
    display: flex;
    align-items: flex-start;
    gap: 16px;
    padding: 20px;
    background: var(--white);
    border-radius: 12px;
    box-shadow: var(--shadow-sm);
    transition: all 0.3s ease;
}

.value-item:hover {
    transform: translateY(-3px);
    box-shadow: var(--shadow-md);
}

.value-icon {
    font-size: 2rem;
    line-height: 1;
    flex-shrink: 0;
}

.value-item h4 {
    font-size: var(--font-size-lg);
    font-weight: 700;
    color: var(--primary-dark);
    margin-bottom: 8px;
    line-height: 1.3;
}

.value-item p {
    font-size: var(--font-size-sm);
    color: var(--medium-gray);
    line-height: 1.6;
    margin: 0;
}

/* Story Visual */
.story-visual {
    display: flex;
    flex-direction: column;
    gap: 32px;
}

.founders-section {
    display: flex;
    flex-direction: column;
    gap: 24px;
}

.founder-card {
    background: var(--white);
    border-radius: 16px;
    padding: 24px;
    box-shadow: var(--shadow-md);
    text-align: center;
    position: relative;
    overflow: hidden;
    transition: all 0.3s ease;
    cursor: pointer;
}

.founder-card:hover {
    transform: translateY(-5px);
    box-shadow: var(--shadow-xl);
}

.founder-card:focus {
    outline: 3px solid rgba(99, 102, 241, 0.6);
    outline-offset: 3px;
}


.founder-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: var(--gradient-primary);
}

.founder-image {
    margin-bottom: 20px;
}

.founder-placeholder {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    background: var(--gradient-primary);
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 auto;
    box-shadow: var(--shadow-md);
}

.founder-initials {
    font-size: var(--font-size-xl);
    font-weight: 800;
    color: white;
    letter-spacing: 1px;
}

.founder-info h4 {
    font-size: var(--font-size-lg);
    font-weight: 700;
    color: var(--primary-dark);
    margin-bottom: 4px;
}

.founder-title {
    font-size: var(--font-size-xs);
    color: var(--primary-energy);
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    margin-bottom: 16px;
}

.founder-info blockquote {
    font-size: var(--font-size-sm);
    font-style: italic;
    color: var(--medium-gray);
    line-height: 1.5;
    margin: 0;
    padding: 0;
    border: none;
}

.story-stats {
    display: grid;
    grid-template-columns: 1fr;
    gap: 16px;
    background: var(--white);
    border-radius: 16px;
    padding: 24px;
